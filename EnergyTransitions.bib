@article{Cox1986,
	Abstract = {It has long been the accepted view that twentieth-century British voters tend to base their vote decisions chiefly upon the partisan affiliations of the candidates, rather than upon the candidates' personal policy beliefs or characteristics; voters are 'party-orientated' rather than 'candidate-orientated'. This article focuses upon three previously unanswered questions: (1) when did English voters become party-orientated? (2) why did they do so? and (3) what were the consequences? After demonstrating statistically that English voting behaviour changed markedly during the nineteenth century (based upon an analysis of over a thousand election contests between 1832 and 1918), the causal relationships between electoral choice and Parliamentary behaviour are examined.},
	Annote = {Major points: 1. That after 1832, there was a gradual shift in Britain to party voting rather than member voting by the electorate 2. That a dominant cause of this shift was the change to private bills' rules in Parliament, that put neutral members rather than interested members in charge. This increase in party power over regulation of commerce and construction of public goods (canals, railways, granting of charters, land planning, etc) meant that the electorate had greater incentive to select on the party that represented their interests rather than the member, because members with conflicts of interest no longer could operate as if these did not exist. NOTE: the private bills issue figures heavily in the construction of the transport infrastructure, and thus with the second half of hte coal transition (when the canals opened up the possibility of inland coal mining). Need to find more about this.},
	Author = {Gary W. Cox},
	Copyright = {Copyright {\^A}{\copyright} 1986 Cambridge University Press},
	Date-Added = {2009-07-10 13:26:24 -0700},
	Date-Modified = {2009-07-10 14:26:37 -0700},
	Issn = {00071234},
	Journal = {British Journal of Political Science},
	Jstor_Articletype = {primary\_article},
	Jstor_Formatteddate = {Apr., 1986},
	Number = {2},
	Pages = {187--216},
	Publisher = {Cambridge University Press},
	Title = {The Development of a Party-Orientated Electorate in England, 1832-1918},
	Url = {http://www.jstor.org/stable/193738},
	Volume = {16},
	Year = {1986},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.jstor.org/stable/193738}}

@article{mokyr2007distributional,
	Annote = {Excellent summary of the literature on the early English industrial revolution. Three key points: 1. First, during the 18th century, Parliament itself changed radically in its ability to make national policy rather than to support corrupt rent-seeking on the part of Members. See Cox (1986) on this change, which Mokyr suggests occurs after the Glorious Revolution that is given so much credit by North and Weingast. 2. Then, Parliament, in the process of consolidating power to itself, begins to implement legislation more consistent with a set of internal principles than with private rent-seeking. Note that this does not mean the end of oligopoly, rent seeking, or legislation for private gain; but it does suggest that attempts to protect old industry or patterns of economic activity via legislation increasingly failed (see wool vs. cotton), keeping the door open to industrial change 3. Finally, most of hte gains prior to 1845 or so went to the landed gentry, who benefitted from increased demand for land brought about by new demands on land (mining, textiles (wool), agriculture (after the Acts of Enclosure), and industrial plants). Thus while the early IR in England brought about major systemic changes, gains were concentrated in a few hands. Most of these were in turn members of Parliament. Things to check: -Cox on changing British parliamentary politics -Blackstone's summary of the laws of England -},
	Author = {J. Mokyr and J.V.C. Nye},
	Date-Added = {2009-07-10 11:13:36 -0700},
	Date-Modified = {2009-07-10 13:14:28 -0700},
	Journal = {Southern Economic Journal},
	Number = {1},
	Pages = {50--70},
	Publisher = {Southern Economic Association},
	Title = {{Distributional Coalitions, the Industrial Revolution, and the Origins of Economic Growth in Britain}},
	Volume = {74},
	Year = {2007},
