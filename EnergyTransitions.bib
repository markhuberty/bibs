@techreport{Bogart2008,
	Abstract = {A new database demonstrates that between 1600 and 1830, Parliament passed thousands of acts restructuring rights to real and equitable estates. These estate acts enabled individuals and families to sell, mortgage, lease, exchange, and improve land previously bound by landholding and inheritance laws. This essay provides a factual foundation for research on this important topic: the law and economics of property rights during the period preceding the Industrial Revolution. Tables present time-series, cross-sectional, and panel data that should serve as a foundation for empirical analysis. Preliminary analysis indicates ways in which this new evidence may shape our understanding of British economic and social history. },
	Address = {Irvine, CA},
	Annote = {Absolutely fascinating. Database of Estate Acts from 1600-1830. These corrected and updated the practice of sustaining settlements, which had governed property rights since the early Stuart period. The authors view the purpose of the acts as clarifying and unclogging the system of property rights, to allow more and better transactions involving property to occur. The sustaining settlements themselves had been poorly documented, complicated, complexly administered, and often unclear about how rights to sale, harvest of timber, lease of mining rights, and other modern economic transactions were assigned. The Estate Acts, which were available via appeal to Parliament, clarified these and permitted trade in real estate to occur. IN PARTICULAR: see the information on timber rights and mining rights. MAY BE USEFUL to get this database, to see what was occurring with rights to coal seams, etc. },
	Author = {D. Bogart and G. Richardson},
	Date-Added = {2009-07-10 15:09:20 -0700},
	Date-Modified = {2009-07-10 16:07:21 -0700},
	Institution = {University of California, Irvine},
	Keywords = {law and economics, england, parliament},
	Month = {October},
	Title = {Estate Acts, 1600 to 1830: A New Source for British History},
	Type = {Working Paper},
	Year = {2008},
	Bdsk-File-1 = {}}

