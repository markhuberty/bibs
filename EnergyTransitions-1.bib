	Jstor_Articletype = {primary_article},
	Jstor_Formatteddate = {Apr., 1986},
	Number = {2},
	Pages = {187--216},
	Publisher = {Cambridge University Press},
	Title = {The Development of a Party-Orientated Electorate in England, 1832-1918},
	Url = {http://www.jstor.org/stable/193738},
	Volume = {16},
	Year = {1986},
	Bdsk-File-1 = {},
