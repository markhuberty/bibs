	Pages = {507--529},
	Publisher = {Taylor & Francis, Ltd. on behalf of the Association of American Geographers},
	Title = {Industrial Restructuring and the United States Coal-Energy System, 1972- 1990: Regulatory Change, Technological Fixes, and Corporate Control},
