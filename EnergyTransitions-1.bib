	Local-Url = {file://localhost/Users/markhuberty/Documents/1%20Berkeley%20Information/Course%20Materials/Spring%202008/PS210-Ziegler/Readings/Wk%2006/DeLong,%20B.-Corporation-1997.pdf},
	Month = {July},
	Title = {The Corporation as a Command Economy},
	Type = {Working Papers Series},
	Year = {1997}}

