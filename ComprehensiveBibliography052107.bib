@article{piven2000pra,
	Annote = {Basic argument: that the changing dynamics of workplace organization and international economies have sparked a change in the "power repertoires" of worker organization, replacing old forms (strikes, slowdowns, etc) with other forms. Examples: 1. Move away from NLRB union elections, towards card-check or employer-neutrality elections 2. Engagement with civil society on proxy issues for labor rights: e.g., sweatshops or product safety problems in 3rd world countries that can be tied to the pursuit of cheap labor 3. A return to forms of the "sit-down" strike that keep workers in control of productive capital but demonstrate dissatisfaction 4. Pressure on third parties (e.g., buyers or suppliers of targeted firms) 5. Sector-wide organization, to prevent defection by workers in individual firms Thus the globalization pressures on the main instigator of the welfare state - labor - have not rendered it completely helpless. Rather, new forms of militancy are developing to deal with the new landscape.},
	Author = {F.F.O.X. PIVEN and R.A. CLOWARD},
	Date-Added = {2008-10-07 10:49:56 -0700},
	Date-Modified = {2008-10-07 10:58:25 -0700},
	Journal = {Politics \& Society},
	Keywords = {levy, welfare state, retrenchment, globalization},
	Number = {3},
	Pages = {413},
	Title = {{Power Repertoires and Globalization}},
	Volume = {28},
	Year = {2000}}

@article{ferrera2008ews,
	Annote = {Two phases of the European welfare state: 1. 1945-70: expansion, stability, and successs 2. 1970+: retrenchment, instability, and mixed results What changed? 1. The end of "embedded liberalism" as capital and goods controls disappeared, exchange rates floated, and catch-up came to an end. See Ruggie (1982) and Keohane (1984) 2. The entry of new low-cost producers into the global economy 3. The globalization of finance. 4. The shift to a post-industrial / post-Fordist economy, which destabilized the employment structure and lowered productivity growth 5. The shift in family structures that put women in the workforce and removed the "free" source of household services. 6. Aging populations and deteriorating demographics, leading to greater funding pressures on standing welfare states. What were the results: 1. Greater autonomy for firms, leading to less control over national tax bases 2. Greater demands on the welfare state for protection against new risks - demands the welfare state (particularly in central and southern Europe) wasn't well-prepared to deal with 3. Greater budgetary constraints for the welfare state (both due to the deteriorating tax base and the greater international financial constraint) What were the responses: 1. Structural adjustment: a move to PAYGO systems and adjustment of incentives for greater efficiency 2. Active, not passive, unemployment policy (job retraining, benefits incentives to take work, etc) 3. Retargeting of resources to those most in need (means testing) 4. Restructuring financing to insulate it from demographic pressures How has this been analyzed: 1. "Recalibration": functional (risk balancing), distributive (in favor of protection of the most vulnerable), discursive (in terms of norms and ways of discussing the purpose of the WS) and politico-institutional (modes and levels of engagement on politics of welfare state adjustment. 2. Power Resources and Pierson: why, when conservatives were in power in most places, did welfare state retrenchment not go further? Pierson's answer: because the welfare state had developed far-flung constituencies much different from those that were instrumental in its founding as described by PR. },
	Author = {M. FERRERA},
	Date-Added = {2008-10-07 09:52:12 -0700},
	Date-Modified = {2008-10-07 10:58:38 -0700},
	Journal = {West European Politics},
	Keywords = {levy, welfare state, retrenchment, globalization},
	Number = {1},
	Pages = {82--107},
	Publisher = {Routledge},
	Title = {{The European Welfare State: Golden Achievements, Silver Prospects}},
	Volume = {31},
	Year = {2008},
	Bdsk-File-1 = {}}

@article{genschel2005gat,
	Annote = {Observes two extremes: 1. Globalization will ruin the tax state (general theories of 1980s/90s globalization, + the Washington Consensus) 2. Globalization does not threaten the tax state at all (countries still retain policymaking autonomy and the size of welfare states doesn't matter to growth or employment) Genschel rejects both. Several changes: 1. Increased sanctions against tax evasion and avoidance 2. Increased (though still meager) international coordination on taxation; but tends towards lowest-common-denominator coordination and has been consistently rejected even in coordinated polities like the EU 3. Transnationalization of hte tax base: greater disputes with multinational corporations about where revenues actually come from and how they are attributed. Problems for the state: 1. Erosion of the corporate tax base amidst internationalization of firms 2. Equity-efficiency problems: lower corporate tax rates may keep jobs at home; but the existence of parallel income tax regimes for labor and capital is difficult to justify politically 3. Maintenance of overall revenue levels becomes difficult. Efforts: 1. Reduce corporate taxes but broaden the tax base (revenue-neutral) 2. Treat corporate and individual income taxes (and capital and labor taxes) as separate issues. 3. OR - reduce income taxes across the board, in order to maintain the equity of the tax system. },
