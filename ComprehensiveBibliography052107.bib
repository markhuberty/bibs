@article{Amsden:2003,
	Abstract = {A rrighi, Silver and Brewer (ASB) argue that levels of income in rich and poor countries have not converged while the share of manufacturing in GDP has (in 1990, for example, the share of manufacturing in sub-Saharan Africa's GDP was allegedly as much as 88 percent of what it was in the ``First World'' [Table 2]!). This proposition may be intriguing, if correct. But the au- thors present no persuasive theory to suggest why it should be so. Manufactur- ing has been at the heart of modern economic growth because of its scale economies, principle of specialization, geographical agglomerations, and high incidence of technological change. Why should not more manufacturing activ- ity in the South lead to greater manufacturing incomes, employment, and con- vergence, even if not complete convergence with Northern incomes given the outward shift of the world technological frontier? },
	Annote = {A persuasive response to Arrighi, et al on the pattern of income convergence and industrialization. The author has three critiques: 1. Some of the methods are flawed. The zero-correlation of the relationship between manufacturing as \% of GDP and income appears to be a statistical artifact. 2. Domestic politics are ignored in favor of blaming the entire problem of Southn development on the global international system. Clearly, the evidence since 1980 is that countries can beat the system 3. The numbers used are problematic. Why are other elements of economic growth - services, government - ignored? Why is manufacturing, and not value-added, the unit of measure? Manufacturing says little about the quality of the goods produced, the origin of their designs, and the possibilities of technology transfer or learning. Finally, the notion of a global economic hegemony that requires poor countries to remain poor in order to deliver a set of services at low cost required by rich northern economies. This is directly contradicted by the modes of recent success in E Asia, most of which are dismissed out of hand, and by the ignorance of the specific cases of middle-income industrial countries in a variety of different continents. },
	Author = {Alice H. Amsden},
	Date-Added = {2008-09-02 10:43:28 -0700},
	Date-Modified = {2008-09-02 10:45:08 -0700},
	Journal = {Studies in Comparative International Development Policy},
	Keywords = {development, dependency theory, zysman, fall 2008, globalization},
	Local-Url = {file://localhost/Users/markhuberty/Documents/BibtexAutofilePapers/Amsden/2003.pdf},
	Number = {1},
	Pages = {32--38},
	Title = {Comment: Good-bye Dependency Theory, Hello Dependency Theory},
	Volume = {38},
	Year = {2003},
	Bdsk-File-1 = {}}

@article{Arrighi:2003,
	Abstract = {This article demonstrates empirically that widespread convergence in the degree of industrialization between former First and Third World countries over the past four decades has not been associated with convergence in the levels of income enjoyed on average by the residents of these two groups of countries. Our findings contradict the widely made claim that the significance of the North-South divide is diminishing. This contention is based on a false identification of ``industrializa- tion'' with ``development'' and ``industrialized'' with ``wealthy.'' Elaborating from elements of Joseph Schumpeter's theory of innovation, Raymond Vernon's prod- uct cycle model, and Pierre Bourdieu's concept of illusio, the article offers an ex- planation for the persistence of the North-South income divide, despite rapid Third World industrialization and despite dramatic changes in the world political-ideo- logical context for development (that is, the shift around 1980 from the ``develop- ment'' project to the ``globalization'' project or ``Washington Consensus''). While emphasizing the long-term stability of the Northern-dominated hierarchy of wealth, the article concludes by pointing to several contemporary processes that may de- stabilize not only the ``globalization project,'' but also the global hierarchy of wealth that has characterized historical capitalism. },
	Annote = {General: Standard tripe about north-south economic divides, the failure of industrialization to bridge the divide, and the need for a new economic order that upsets the hegemony/oligarchy of the developed countries and in particular the United States. Ignores: 1. The actual change in incomes inside the developing countries, and whether there is a sign that the trend in income improvements mirrors that of the first IR or not 2. The variability inside the developing world. East Asia is generally given short shrift as an example; so is Chile and S Africa. 3. Aggregation of the "developed world" includes Africa, which has major political issues, and instead treats the general problem of economic modernization as generalizable across countries. The availability of measures like "share of mfg in the economy" and "ratio of income in 3rd world to income in 1st world" doesn't tell us much about what's actually going on. The article thus tells us something we already know - that development has proven to be difficult - but overlays it with a general critique of economic development policy in general, which is not necessarily good. Notably, lots of success has been had by some nations mostly in Asia, but SE Asia and China and E Asia are different things - by deviating from this, without a lot of punishment from the "economic hegemons."},
	Author = {Giovanni Arrighi and Beverly J. Silver and Benjamin D. Brewer},
	Date-Added = {2008-09-02 10:15:48 -0700},
	Date-Modified = {2008-09-02 10:18:11 -0700},
	Journal = {Studies in Comparative International Development Policy},
	Keywords = {development, zysman, fall 2008, globalization},
	Local-Url = {file://localhost/Users/markhuberty/Documents/BibtexAutofilePapers/Arrighi/2003.pdf},
	Number = {1},
	Pages = {3--31},
	Title = {Industrial Convergence, Globalization, and the Persistence of the North-South Divide},
	Volume = {38},
	Year = {2003},
	Bdsk-File-1 = {}}

@article{Little:2000,
	Abstract = {Abstract A central problem in public health studies is how to make inferences about the causal effects of treatments or agents. In this article we review an approach to making such inferences via potential outcomes. In this approach, the causal effect is defined as a comparison of results from two or more alternative treatments, with only one of the results actually observed. We discuss the application of this approach to a number of data collection designs and associated problems commonly encountered in clinical research and epidemiology. Topics considered include the fundamental role of the assignment mechanism, in particular the importance of randomization as an unconfounded method of assignment; randomization-based and model-based methods of statistical inference for causal effects; methods for handling noncompliance and missing data; and methods for limiting bias in the analysis of observational data, including propensity score matching and sensitivity analysis. },
	Author = {Rodrik J. Little and Donald B. Rubin},
	Date-Added = {2008-08-31 14:43:56 -0700},
