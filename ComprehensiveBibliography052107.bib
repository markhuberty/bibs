@inbook{Thelen2006,
	Address = {Cambridge},
	Annote = {Problems: 1. Various theories are useful at explaining why institutions exist in the form they do (culturalist, functionalist, ) 2. Other theories can explain why institutions survive and reproduce, or why they undergo radical punctuated change (path dependency) But how do we understand institutional evolution, that is, slow institutional change through time, as we witness in many areas? How do institutions meant for one purpose at one time end up doing something very different at another time? Two answers: 1. Layering: the ruleset of the institution is added to or supplemented with new rules that permit it to fulfill a given purpose in a new way. This may change the coalitions that participate in the institution or change their position, such that the institution takes on a new form for a new configuration of actors. 2. Conversion: take existing forms of social-political organization and turn them to new purposes. The institutional form remains the same, but hte coalitions served, the tasks done, etc, all change. Question is how closely structure ties to function. },
	Author = {Kathy Thelen},
	Chapter = {6: How Institutions Evolve},
	Date-Added = {2007-10-31 10:40:29 -0700},
	Date-Modified = {2007-10-31 12:58:38 -0700},
	Editor = {James Mahoney and Dietrich Rueschemeyer},
	Keywords = {fish, ps200w10},
	Publisher = {Cambridge University Press},
	Title = {Comparative Historical Analysis in the Social Sciences},
	Year = {2006}}

@article{Weldon2006,
	Abstract = {Drawing on recent insights in the nationalism and citizenship regime literatures, this article develops a macrotheoretical framework for understanding cross-national variations in tolerance of ethnic minorities. Specifically, it tests the hypothesis that the degree to which the dominant ethnic tradition or culture is institutionalized in the laws and policies of a nation-state affects citizen tolerance of ethnic minorities. Employing a multilevel regression model, it systematically tests the framework, as well as competing individual and country-level explanations, for all member states of the European Union in 1997. Results confirm a strong relationship between the laws governing the acquisition and expression of citizenship, that is, citizenship regime type, and individual tolerance judgments. Moreover, citizenship regime type has a strong mediating effect on three individual-level variables previously shown to predict tolerance: ingroup national identity, political ideology, and satisfaction with democracy. },
	Annote = {Argument: that individual tolerance of immigrants and minorities is closely shaped by the form of citizenship law in place in the state. Weldon sees citizenship law as a potential means of institutionalizing the dominant ethnic tradition or culture. Notes that strong ethnic identification is not always the source of discrimination or intolerance in societies. Thus why is it in some but not in others? Three possible regime types: 1. Collectivist - ethnic (jus sanguinis citizenship laws) - not accepting unless groups are linked by birthright 2. Republican / assimilationist - accepting to the extent that new groups can adopt the characteristics of the dominant culture 3. individualist - civic (multicultural / no assimilation) - ``state'' more important than ``nation'' - the nation is viewed not as a collection of groups but as one of individuals with a right to unique belief structures. Three hypotheses: 1. Collectivist-ethnic groups will show low levels of both soial and political tolerance 2. Republican states will show political tolerance but not social tolerance 3. Individualist states will show both. Confirmed by analysis of a Eurobarometer survey across Europe measuring both social and political tolerance. Conclusion: controlling for economic prosperity, growth, and other factors, citizenship regime type is the central determining factor for levels of political and social tolerance in a society.},
	Author = {Steven A. Weldon},
	Date-Added = {2007-10-31 10:27:56 -0700},
	Date-Modified = {2007-10-31 10:29:20 -0700},
	Journal = {American Journal of Political Science},
	Keywords = {fish, ps200w10},
	Month = {April},
	Number = {2},
	Pages = {331--349},
	Title = {The Institutional Context of Tolerance for Ethnic Minorities: A Comparative, Multilevel Analysis of Western Europe},
	Volume = {50},
	Year = {2006}}

@article{Helmke2004,
	Abstract = {Mainstream comparative research on political institutions focuses primarily on formal rules. Yet in many contexts, informal insti- tutions, ranging from bureaucratic and legislative norms to clientelism and patrimonialism, shape even more strongly political behavior and outcomes. Scholars who fail to consider these informal rules of the game risk missing many of the most important incentives and constraints that underlie political behavior. In this article we develop a framework for studying informal institutions and integrating them into comparative institutional analysis. The framework is based on a typology of four patterns of formal- informal institutional interaction: complementary, accommodating, competing, and substitutive. We then explore two issues largely ignored in the literature on this subject: the reasons and mechanisms behind the emergence of informal institutions, and the nature of their stability and change. Finally, we consider challenges in research on informal institutions, including issues of identification, measurement, and comparison. },
	Annote = {Argument: that societies have informal institutions that play critical roles in how politics works; that these roles can be either complimentary or destructive to the operation of the formal institutions; and that therefore understanding a country's informal institutions is critical to understanding how politics in its formal institutions works. Possibilities for informal institutions: Convergent on rules observed by effective formal institutions: complimentary Divergent on rules observed by effective formal institutions: accommodating Convergent on rules observed by ineffective formal institutions: Substitutive Divergent on rules observed by ineffective formal institutions: competing Thus informal rules can bolster the effectiveness of formal institutions, and can either compensate for weak formal structures or erode them. It is not a simple question of functional/dysfunctional. Origins of informal institutions: 1. Incompleteness of formal structures 2. ``Second best'' strategy for those who cannot achieve their aims via formal institutions 3. Pursuit of goals not considered publically acceptably and therefore goals that can't be embodied in formal institutions Process of creation: attention should be paid to the process of creation using the same method of analysis as for formal institutions: whose interests are served? how are those interests conceived? What deals were made between interest groups in order to found a particular set of rules? Three research questions: First, what are the actors' shared expectations about the actual constraints they face? Second, what is the commu- nity to which the informal rules apply? Third, how are informal rules enforced? },
	Author = {Gretchen Helmke and Steven Levitsky},
	Date-Added = {2007-10-31 10:16:33 -0700},
	Date-Modified = {2007-10-31 10:17:47 -0700},
	Journal = {Perspectives on Politics},
	Keywords = {fish, ps200w10},
	Month = {December},
	Number = {4},
	Pages = {725},
	Title = {Informal Institutions and Comparative Politics: A Research Agenda},
	Volume = {2},
	Year = {2004}}

@article{Kuenzi2007,
	Abstract = {Many people consider voting the most important form of political participation in a democracy. To this point, no aggregate-level, cross-national studies have been done on the factors that affect voter turnout in Africa. This article seeks to fill the gap in the literature by examining the factors that influence voter turnout in sub-Saharan Africa's multiparty regimes that have had two consecutive elec- tions since the democratic transitions in the 1990s. Many of the central findings of the research on voter turnout in other regions reappear in the examination of voter turnout in Africa. The authors find that two institutional variables---type of electoral formula and concurrency of presidential and legislative elections--- have significant effects on electoral turnout. Media exposure has a significant positive relationship with voter turnout. The number of elections a polity has had also appears to affect levels of voter turnout.},
	Annote = {Controlling for other factors - economic development, racial diversity, etc - the authors find that voters in Africa respond to institutional factors in ways similar to voters in other regions. Hence evidence suggests that voting behavior is similar across regions and that there is nothing unique in African voting behavior. Another way of asking the question: will Africa's very different informal institutions make voter behavior much different from other regions? Method: Look at voter turnout as a \% of registered voters in legislative elections. Institutional variables: party diversity; majoritarian vs. proportional representation; bicameral vs. unicameral Control variables: level of democracy (FH scores); time since democratization; presidential power; socioeconomic variables (urbanization, pc income, pc income growth); media coverage. Results: 1. Institutional design matters along the following lines: -Simultaneous elections matter -Majoritarian systems have lower turnout -Multiparty systems appear not to matter -Previous experience in elections (e.g., longer time since democratization) does matter 2.Net: First, African's arent' different; second, they respond in ways we'd expect from observing other democracies; third, thre is a time lag in institutional effectiveness. },
	Author = {Michelle Kuenzi and Gina M. S. Lambright},
	Date-Added = {2007-10-31 10:02:02 -0700},
	Date-Modified = {2007-10-31 10:03:49 -0700},
	Journal = {Comparative Political Studies},
	Keywords = {fish, ps200w10},
	Pages = {665},
	Title = {Voter Turnout in Africa's Multiparty Regimes},
	Volume = {40},
	Year = {2007}}

@inbook{Huntington1968,
	Address = {New Haven:},
	Annote = {Question: how to explain the political instability of the developing world? Hypothesis: Economic development produces rapid social change and social modernization that destabilizes more slowly developing political institutions. ``If men are to remain civilized or to become so, the art of associating must grow and improve in the same ration in which the equality of conditions is increased.'' Causal mechanism: social and economic change produce rising political consciousness and extend political demands; but the institutions to manage these demands are underdeveloped. The mismatch between political mobilization and political effectiveness creates political instability. Effect on development policy in the postwar era: Americans tend to assume that economic change produces political stability; but they don't provide a way to manage that change, and so end up producing instability. In contrast, the communists provide a way of managing the political gap - even if their economic proposals are less useful. Hence their attractiveness. Role of political institutions: they facilitate the ``coming together'' of disparate social groups and channel competition or disagreement between these groups into a bargaining process. Measurement of institutionalization: four factors: adaptability, complexity, autonomy, and coherence Adaptability: can the institution change in response to changing external conditions, or is it designed for only one set of circumstances? Complexity: how many purposes does an institution serve? Those that serve many - or interlinked - purposes are both more likely to survive and more indicative of a complex society with a high level of institutionalization. Moreover, these more complex institutions are, in turn, likely to be more adaptable to changing circumstances. Autonomy: Institutions should not be captured by one social group or set of social groups. Rather, they need to function as a way of managing social tensions generally. To the extent that they are not autonomous, they will tend to be less adaptable / more likely to lose their ability to manage conflict as the environment changes. Also - if politics is not autonomous, it enables groups that have captured institutions to play at politics without being identified as doing so, since they can play through the institutions they manage. Hence the institution is failing at the crucial role of bringing social groups into the political system where they are forced to associate with each other. Coherence: What are the boundaries of an institution's authority or function? How well-structured is it to resolve conflicts within these boundaries? POLITICAL INSTITUTIONS AND PUBLIC INTERESTS Argues that ``the public interest'' does not exist without institutions, since there is no entity with a long enough time horizon to conceive of the public interest, and there is no place where various private interests can converge. But an autonomous institution can have a sufficiently long time horizon to conceive of a public interest and implement it. MODERNIZATION AND DECAY Social mobilization + economic development tend to upset institutions. These changes occur faster than the institution can adapt to them. As a result, it becomes difficult to (1) manage conflict between groups and (2) conceive of a public interest beyond the various factional interests. Moreover, many early institutions - tribes, villages, religions - are incapable of managing the kind of complex society produced by economic modernization. The failure of old institutions before new ones are created tends to produce ``alienation and anomie, normlessness generated by the conflict of old and new values...before new skills, motivations, and resources can be brought into existence'' to create new ones. As a result, people tend to look to older, possibly obsolete groupings to anchor their lives. ``Modernity breeds stability, but modernization breeds instability.'' Factors to consider: breakdown of old social groups; rising income inequality; increases in mobility, diluting social ties; distributional conflicts; rising education and literacy, enabling better communication and knowledge; better capacity for group organization; rising middle class with demands on political power. Economic growth may pacify these trends for awhile; but economic downturn tends to exacerbate them and cause revolution.},
	Author = {Samuel Huntington},
	Chapter = {1: Political Order and Political Decay},
	Date-Added = {2007-10-31 09:36:20 -0700},
	Date-Modified = {2007-10-31 09:38:09 -0700},
	Keywords = {fish, ps200w10},
	Publisher = {Yale University Press},
	Title = {Political Order in Changing Societies},
	Year = {1968}}

@techreport{Frieden2003,
	Address = {Brussels},
	Annote = {Question: global capitalism failed once when at the height of its transnational integration. It could do so again. What do we need to learn from the first failure in order to forestall the second? 1. Global capitalism failed the first time because its major distinguishing features - the gold standard chief among them - were too rigid to permit national adjustment to the problems of the first part of the 20th century. This was mostly true in the prewar period. In the interwar period, it was especially true - because of the rise of highly democratic systems. The Gold Standard required deflation as a way of restoring balance; deflation is relatively easy to tolerate if you can ignore the populace, but much harder if you cannot. Hence the problems of hte interwar years, when the franchise was radically extended. (Note that the Economist Review on ``The Titans'' suggests that deflation isn't so bad...if originating from technological change and not monetary policy. But how does that account for the 1873-96 period, when rising ag prod and falling demand costs created what was known as the ``Great Depression?'' Remember, viz Keynes, that both deflation and inflation have income-redistributional effects. 2. The solution to this was embodied in three major settlements - security, economic, and social/political - that permitted flexibility within a system of macroeconomic stability. Doing so was made possible by the critical link between security and economics in the Cold War, which led the U.S. to make certain compromises on principle in order to achieve good outcomes. 3. As the security-economics link fails, and the hegemonic role of the U.S. fades, how to manage the adjustment? Lessons: 1. Ignoring the costs and tensions of globalization at the national level, in particular those experienced by its losers, will be fatal to the enterprise. Likewise, ignoring the benefits of globalization will also be fatal. The faiure to do both was at the root of the failure of the interwar economy and the inability to reconstruct a stable macroeconomic order. 2. Market forces will not solve all needs. But neither will autarky. 3. Institutions are needed to help manage adjustment Signs of emerging macroeconomic tensions: 1. Rising income inequality in the United States, despite continued good economic performance 2. Growing dependence of the main world economy on imported capital, with unpredictable effects on macroeconomic adjustment 3. The end of a significant national security reason for the US to pursue open international markets (unlike in the Cold War) - which formed the basis for the bipartisan commitment to open markets (which brought in protectionist republicans via their Cold War credentials, and the more liberal free-traders despite their ambiguous commitment to the Cold War). In fact, now you might notice tensions between security and economics - as with the economic dependence on Chinese capital despite potential security problems. },
	Author = {Jeffry Frieden},
	Date-Added = {2007-10-30 14:39:31 -0700},
	Date-Modified = {2007-10-30 14:51:36 -0700},
	Institution = {Breugel},
	Keywords = {zysman, ps201w10},
	Title = {Will Global Capitalism Fail Again?},
	Type = {Bruegel Essay and Lecture Series},
	Year = {2003}}

@article{Steinberg2002,
	Annote = {Question: 1. Why would powerful countries like the EC or US support an organization that makes hard, enforceable law, if they sit atop the power structure? 2. How have the EC and US dominated the GATT / WTO in the face of its consensus-based decisionmaking process 3. If the answer to (2) is that they simply dominate the power relationships, then why bother with the institution in the first place. Argument that the EC and US set the terms of the WTO because they control power in both institutional and world-economic terms and therefore can place demands on countries that cost little for the EC and US but promise relatively large gains for the developing economies. That power is embodied in the agenda-settiing process, post-agenda setting rules modification, and treaty finalization. Hence theWTO / GATT rules are dependent on a power structure for solvng the coordination problem. To the extent that the EC and US can continue to do so, the stage is set for further integration. Btu if the EC and US fail to find common ground, or themselves revert to a nation-centered staregy.},
	Author = {Richard H. Steinberg},
	Date-Added = {2007-10-30 14:17:48 -0700},
	Date-Modified = {2007-10-30 14:19:08 -0700},
	Journal = {International Organization},
	Keywords = {ps201w10, zysman},
