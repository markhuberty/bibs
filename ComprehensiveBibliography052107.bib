@book{Shapiro1996,
	Annote = {Argument for democracy being a ``subordinate foundational good'' to some other set of higher social goods, on the ground that striving for democracy does little for the development of human dignity. Rather, each society should conceive of democracy as a way of pursuing some (society-defined) goal of a just society. Basically, comes down to two things: first, the inability to define hard limits to politics; thus it is hard to say, a priori, that anything is off limits. Second, that conflict in society over social outcomes is inevitable, and that democracy is probably the best way to resolve them (since other things like hierarchy and such, are ultimately bad). Hence democracy isn't purely procedural - but it's not purely substantive, either. Rather, it's in between, process in pursuit of better substance. },
	Author = {Ian Shapiro},
	Date-Added = {2007-11-14 09:07:26 -0800},
	Date-Modified = {2007-11-14 09:08:03 -0800},
	Publisher = {Cornell University Press},
	Title = {Democracy's Place},
	Year = {1996}}

@book{Bobbio1987,
	Address = {Minneapolis},
	Annote = {What Alternatives are there to Representative Democracy? -In a word, none. Democracy has four internal paradoxes: 1. Compared to the ``ancients'', we want more democracy under less opportune conditions (conflict with Constant?) 2. Ever-more-complex society demands that we have more experts, but expertise is inherently undemocratic. Moreover, as the franchise expands (more democracy) the demands on the state expand (more bureaucracy). This doesn't make it bad, but it does mean that as we become a more advanced society, we face a choice: to become more technocratic, or to yield more complex decisions to less-well-informed people on the grounds of democracy 3. Technological advance has the same effect as (2). 4. Democracy depends on an open mass society, but mass society seems to converge on cultural and political ``industries'' that inform against hte individual agent. Alternate models: It is difficult to conceive of other models that attempt to guarantee the kind of rights-based framework that democracy (particularly liberal democracy) does. Calls for achievement of some kind of socialist democracy via revolution and dictatorship are misguided: democracy cannot be had via dictatorship, which often proves permanent rather than transitory and oppressive rather than liberal. Direct democracy (as with Constant) is obtainable only in the case of small polities. Finally, the one feature of liberal market democracy of detriment to the last is the diminished sovereignty of the people in the realm of economic decisionmaking, which is removed from the domain of politics. },
	Author = {Norberto Bobbio},
	Date-Added = {2007-11-14 08:53:06 -0800},
	Date-Modified = {2007-11-14 08:54:34 -0800},
	Keywords = {fish, ps200w12},
	Publisher = {University of Minnesota Press},
	Title = {Which Socialism? Marxism, Socialism, and Democracy},
	Year = {1987}}

@inbook{Mendez2004,
	Address = {South Bend},
	Annote = {Critiques of O'Donnell: The gradual expansion of the domain of rights (Rorty's ``rights talk'') threatens to run into conflict with the idea of the sovereignty of the people. Human rights are naturally majoritarian, in that the circumscribe the accepted domain in which the majority rules, by setting up areas of the law the majority is not allowed to touch. But with an ever-expanding set of rights, those areas grow ever larger. If rights move out of the political / civil realm into the social / cultural / linguistic / etc realm, then large swaths of society remain outside political consideration and democratic rule. Second, you can describe democratic accountability in a 3-fold manner: accountability of the state to the polity (voting); accountability of the polity to itself (legal / criminal), and accountability of the state to itself (horizontal - e.g., checks and balances). It's a mistake to forget the last. Latin America has reasonable voting, sketchier criminal law, and very bad horizontal accountability. },
	Author = {Juan Mendez},
	Chapter = {Fundamental Rights as the Limit to the Democratic Principle of Majority Will},
	Date-Added = {2007-11-14 08:39:05 -0800},
	Date-Modified = {2007-11-14 08:39:05 -0800},
	Editor = {Guillermo O'Donnell and Jorge Vargas Cullell and Osvaldo M. Iazzetta},
