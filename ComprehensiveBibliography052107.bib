@article{Lapidus1998,
	Annote = {The author argues that the war in Chechnya was deliberately orchestrated by Russia from Moscow, that it did not represent the outcome of failed attempts at non-violent conflict resolution, and that in this Moscow was aided and abetted by a West more concerned about supporting the Yeltsin government against perceived communist/nationalist threats than about supporting the evolution of legitimate political institutions. The result was a war of miscalculation on all levels: by Moscow, which seriously underestimated the opposition forces and thus was dragged into a bloody standoff; by Washington, which mischaracterized the threat to Yeltsin and so failed to bring an end to a war that ultimately weakened him; by the UN, which missed signals for an oncoming war. Major factors: 1. The weakness of institutions. Post-Soviet Moscow had not consolidated central control over major foreign policy and military institutions. As a result, nationalist/communist elements could turn these institutions against Yeltsin, ensuring the presentation of a specific interpretation of the conflict (e.g. the dissolution of Russia at the hands of a criminal band) and specific solutions (military action). 2. The misunderstanding of the ethnic/cultural situation. Lacking real knowledge about the history of Chechnya (including mass deporation under Stalin and repatriation under Khruschev), the West misunderstood what was going on in Chechnya. That, and a missed link between the Russian portrayal of Chechnya, and Russian neo-imperialist attitudes more generally. 3. The priorities of the West. Moscow was a critical partner in NATO enlargement, Bosnia, nuclear non-proliferation, and other security initiatives. Yeltsin had a close relationship with Clinton. Thus in this context, with Yeltsin perceived to be weakening, the West chose Moscow over a confrontation with the Russians on Chechnya. 4. The Russian framing of the debate in the West. Portraying the Chechens as criminals / terrorists / insurgents fit with the Western perception of the situation and went largely unquestioned by major governments. Likewise, the Western concern that the loss of Chechnya could spark a dismantling of Russia was reinforced by official statements from Moscow. Prospects: 1. Lack of a negotiated settlement has (a) destabilized Chechnya internally, for it lacks legitimacy either as an autonomous state or as an independent country, and (b) made further negotiation difficult. 2. Settlement of territorial issues is unlikely given the proto-nationalist climate in Moscow. Economic options are perhaps more likely. 3. The West now has little influence, since it did not involve itself in the beginning and did not support organizations like the OSCE which attempted to involve themselves in the middle, to Russian opposition. },
	Author = {Gail W. Lapidus},
	Date-Added = {2007-05-21 09:08:23 -0400},
	Date-Modified = {2007-05-21 09:08:23 -0400},
	Journal = {International Security},
	Number = {1},
	Pages = {5--49},
	Title = {Contested Sovereignty: The Tragedy of Chechnya},
	Volume = {23},
	Year = {1998}}

@article{KHADURI2005,
	Annote = {Georgia faced several problems at the outset of economic reform: 1. A lack of any prior experience in running its own economy, due to 200 years of relations with Moscow / St. Petersburg 2. A running war with Abkhazia on its territory, disrupting the reform process 3. The lack of prior existing financial and political institutions, which had to be built even as the existing economy was being reformed 4. The resolution of the Abkhaz war, which cut Georgia off from its traditional Russian markets Those problems were exacerbated by slow and misguided reform: 1. Politics took precedence over economics in the 1990-1991 period, delaying crucial reforms while other post-Soviet states moved on 2. Freeing of price controls was soon followed by their re-imposition: goods quickly flowed out of hte country 3. Land reform distributed somewhat productive collective land to wholly unprepared individual farmers who lacked the capital to make the smaller farms productive. Georgia's self-sufficiency and its export potential dropped. 4. Georgian independence did not come with control of its money supply, depriving it of a key policy tool 5. Government revenues routinely exceeded government budgets, causing a net contractionary effect Later attempts at reform, 1994-1995, only made this worse: 1. A substitute currency was issued, but the ruble remained; the economy became dollarized while the new currency was inflated out of use. 2. Hyperinflation resulted from subsidization of agriculture and industry 3. International trade by barter opened the economy to corruption and resulted in huge public debts from subsidizing goods needed for export barter. 1995-1998: IMF intervention and some reform 1. But voucher privatization was corrupt and resulted in the looting of industry for saleable capital goods 2. Little democratic institutional reform; Georgia was de facto an autocracy from 1995 onward after consolidation of the roles of head of state and chief executive in one office. This would not change until the Rose Revolution of 2004 3. Governmental needs to fund pensions and wages led to borrowing at very high rates - 24\%+ - that increased with each new bond issuance. This crowded producers out of the market and made private investment impossible. 4. Mismatch between laws and institutions: e.g. bankruptcy, which is enshrined in law but effectively nonexistent in practice. 5. Role of law in making survival a matter of corruption - thus embedding corrupt practices in society 6. Energy sector burdened with debt to meet its obligations to the European energy markets, but now cannoy borrow effectively. Conclusion: Pre-2004 was largely a loss to Georgia. And the legacy of this time - heavy indebtedness, embedded corruption, unproductive industry, and a malfunctioning government - will bear on the country despite its new reform-oriented leaders. },
	Author = {NODAR KHADURI},
	Date-Added = {2007-05-21 09:08:23 -0400},
	Date-Modified = {2007-05-21 09:08:23 -0400},
	Journal = {Problems of Economic Transition},
	Number = {4},
	Pages = {18--29},
	Title = {Mistakes Made in Conducting Economic Reforms in Postcommunist Georgia},
	Volume = {48},
	Year = {2005}}

@unpublished{Cornell1999,
	Annote = {Question: what was the role of nationality under communism? Lenin: -Saw national unrest within tsarist Russia as a tool to use in promoting revolution -Oppression of minorities was incompatible with socialism and therefore would not be tolerated -Officially endorsed autonomy of nationalities, arguing that national self-determination did not per se mean secession -But emphasized the necessity for the centralized party structure of Communism to support the revolution of the proletariat USSR: -Creation of ethnically-based SSRs, oblasts, and autonomous regions often proceeded without real evidence of enduring ethnic identities among the citizens of the newly-constituted entity, and cutting through close ethnic ties -Thus 'nationalism' and 'national self-determination' used as a tool for divide-and-conquer ruling -Death of Lenin and rise of Stalin marked a turn towards 'Russification' and attempts to break down old national boundaries -During the 1930s the national question was 'solved' via purges of anyone demanding equal rights for non-Russian minorities -The war saw ethnic minorities fight with the Germans against the Russians, a sign that nationalism still remained below the surface. The result was mass deportation, privation, and genocide to remove the most troublesome populations, as in Chechnya, Georgia, and other areas of the southern USSR. -Khruschev reversed the Russification policy initially, as a way of legitimating his power and purging the Stalinist elites. But the contradiction remained: nationalism was incompatible with the Party-centric structure of the USSR. Thus the later period saw a return to centralized rule and restriction of autonomy. -Brehznev turned the ethnic states into clients through favoritism and exploitation of a client-patron relationship with the elites of each state. Communism began to fall victim to the demands of Soviet cohesion. -Gorbachev returned to the idea of political pluralism, and with it, reduced the role of ideological rhetoric. But this was counterproductive for elite loyalty, and the structure of autonomy now gave local elites reasons to preserve their own power by acting as agents of nationalism. },
	Author = {Svante E. Cornell},
	Date-Added = {2007-05-21 09:08:23 -0400},
	Date-Modified = {2007-05-21 09:08:23 -0400},
	Note = {PhD Thesis, Ch 3},
	Title = {Small Nations and Great Powers},
	Year = {1999}}

@article{King2001a,
	Annote = {Question: What is the relationship between war, business, and state-building in the Post-Soviet Caucasus? Wars of the Soviet succession:Nagorno-Karabakh,Transnistria,South Ossetia, Abkhazia, Chechnya, Tajikistan, all having run their course by the middle of the decade except for Chechnya. But territorial conflicts remain, though not expressed in violence - how to explain the lack of violence despite the persistence of conflict? Two theses: (1) that the wars of Soviet succession were relatively successful in state-making, and (2), that the disputes have attained an equilibrium because of interests in maintaining the status quo, despite its unsatisfactory elements. Theories on propagation of civil war: (1) Ethnic/nationalist sentiments prevent negotiation on grounds of avoiding 'national betrayal' that get stronger as war losses go on, and (2) wars are profitable for powerful interests on each side, even if they aren't good for the belligerents as a whole. In the last case, post-victory attempts to build institutions are corrupted by the perpetuation of these powerful interests within the political landscape, reducing politics to a zero-sum game. But - in the Caucasus, this has occurred with the collusion of central governments, not just the misbehavior of elites, essentially co-opting otherwise separatist groups into building the postwar, multi-ethnic states that have resulted in the region. Ex 1: Armenia-Azerbaijan war over the status of Karabakh, an autonomous part of Armenia during the Soviet era which was ethnically Azeri. Fought between 1989-1995 to a status quo with Armenia in control of Karabakh. Ex 2: Moldova-Transnistria war over Transnistrian resistance to Moldovan separatism (where the Transnistrian portion of Moldova wished to stay with Russia due to strong economic connections). Fought between 1990-1992 to a stalemate where Transnistria is a protectorate of Russia but no formal treaty exists. Ex 3: Georgia-Abkhaz war over Georgian control of the formerly autonomous republic. Fought from 1992-1994 to a stalemate where Abkhaz is formally autonomous but with significant Russian support, with negotiations contributing little to final status. Ex 4: Georgia-South Ossetian war over the Ossetians' resistance to becoming part of Georgia given ethnic and linguistic differences. Fought to a standstill between 1989-1992, leading to final status negotiations via the OSCE. Why no final settlements in any of these conflicts despite pressure from the OSCE and Russia? Answer: because the longer no settlement is reached, the longer the de facto states in the contested regions have to solidify their (largely effective) control, and the longer interested elites have to take advantage of the political structures for other reasons. Economics - the persistence of local governments in contested regions has allowed economic rent-taking, as in customs duties, local control of large plants (like Transnistrian steel), and local control of agricultural output. Thus new interests have arisen to support autonomy rather than final settlement. Russia has expressed standing interests in the autonomous regions, part of its ongoing disputes with the Caucasus nations. Security Services have given aid to the regions, withdrawals of Russian troops from the Caucasus have been predicated on resolution of the autonomous areas, and Russia maintains strong economic links that give the areas sustainance. Visa policy also favors the regions compared to their Caucasus adversaries. ``Governments'' in the autonomous regions have begun coordinating to limit the influence of the Caucasus nations in any of them - consolidated foreign policy gives them strength. And most of the parties involved benefit from the unresolved status: -The Russians, as a pawn in their political battles against the former Soviet republics in the Caucasus -Certain elites within the Caucasus republics, who have gained political power either from the conflict itself or from exploitation of the economic opportunities implicit in weak states and humanitarian aid -The autonomous regions, who would lose power if incorporated into another nation or if forced to become more legitimate. Meanwhile, these pseudo-states have become centers of traficking in arms, drugs, people, and goods across Europe. And the embedded interests make it difficult for even the most objective, disinterested negotiator to resolve the problem. Thus the problems of these areas aren't just security or strategic - they are economic and political as well, embedded in the quasi-settlements of the postwar era. },
	Author = {Charles King},
	Date-Added = {2007-05-21 09:08:23 -0400},
	Date-Modified = {2007-05-21 09:08:23 -0400},
	Journal = {World Politics},
	Pages = {524--52},
	Title = {THE BENEFITS OF ETHNIC WAR Understanding Eurasia's Unrecognized States},
	Volume = {53},
	Year = {2001}}

@article{Cornell2006,
	Annote = {Cornell argues that, while Azerbaijan has not traditionally been a haven for radical Islam (and in fact was among the leading secular Muslim nations in its region, and rather pro-Western), the signs of an increasing role for political Islam are unmistakeable. His argument is that Azerbaijan lived in an area of religious convergence - Christian to the West, Muslim to the South, Buddist and Zoroastrian to the East - that meant that its religious identity was uniquely Azeri, rather than identified with any one center. During the period of Russian colonialism and then Soviet control, this religious identity faded away under forced aethism, but has returned since 1989. The problem is that what has returned has largely been Islam, though not initially radicalized. Rather, the West's handling of the war in Karabakh and its punishment of Azerbaijan despite it not being the aggressor, combined with the war in Iraq and other problematic relations, have tempted Azeris to adopt the more theocratic idea of Islam. While not yet a majority, the potential role of Azerbaijan as a potential point of Islamic radicalism should not be underestimated. },
	Author = {Svante E. Cornell},
	Date-Added = {2007-05-21 09:08:23 -0400},
	Date-Modified = {2007-06-09 10:15:54 -0400},
