@techreport{Rodrick2004,
	Address = {Cambridge},
	Annote = {Argument: Contra neoliberal theories, industrial policy is more relevant than ever to the successful development of emerging markets. What's changed is the content of that IP and the demands it places on governments. Recognizing this has led to a convergence between economic theories focusing on market failures needing government solutions, and those focusing on market efficiencies from which government should abstain. Argument: First, that IP is about figuring out where market failures present barriers to effective development, and then using policy to resolve them. These failures are NOT located primarily in industry. Second, that IP now is NOT about picking winners; in fact, emerging markets in global economies often find their competitive advantage by experimentation. Thus the real goal is to encourage not industrial concentration but industrial experimentation, such that the search for competitive advantage proceeds apace. Third, that mistakes are NOT bad. What is bad is not recognizing them and escaping from them. Bureaucracies should not be punished for mistakes, but should be punished for entrenching those mistakes. Fourth, that the ultimate problem of industrial innovation in emerging markets is entrepreneurship - demand for innovation - because of the high risk of entrepreneurial activity amidst significant uncertainty that cannot be accommodated by markets. Substance: Entrepreneurship is about cost discovery. Emerging markets can now find niches in which to specialize in the global economy in ways not possible 20 years ago. Those niches involve cost competitiveness. But where such competitiveness exists is not obvious. Thus entrepreneurship is about looking for it, which is inherently risky. Furthermore, unlike other kinds of discovery, you can't protect it: once you figure out where your economy is cost competitive, everyone can move to that sector. So there's a market failure in awarding the cost discovery process. Entrepreneurship must be supported by infrastructure - but keep in mind that ``support'' and ``subsidy'' are not the same. Presumably, if the industry is competitive in international markets, the infrastructure should eventually pay for itself. Thus what's needed is early insurance against entrepreneurial risk, not long-term subsidy. In fact, the subsidy can and will distort the market if incorrectly applied. Cannot specify the institutional structure needed to pull this off. Rather, can describe the continuum of possibilities for hte bureaucracy. Similar to Breznitz, there's a tension between autonomy and embeddedness: or, rather, between the problem of information completeness versus the problem of corruption. How to balance this tension varies with the economic and institutional environment you operate in. Better to have second-best policies administered by competent bureaucracies than first-best ones administered by corrupt or inefficient bureaucracies. Thus don't worry too much about the specific ministry that does the policy - worry about which is the most competent, and design your policies around its competency. Principles: 1. Provide incentives only to ``new'' activities 2. Establish clear benchmarks for success, and use them ruthlessly to avoid entrenching failure 3. Build in sunset clauses to prevent long-term market distortion 4. Target activities, not sectors. It's where the market failures occur and avoids the necessity of picking winners. 5. Subsidize activities with spillover effects. Remember, diversity is the goal. 6. Vest authority in competent agencies even if they aren't necessarily the ``right'' ones. 7. Agency leadership and their bosses must have both clear authority and clear accountability 8. IP agencies must be in communication with the private sector 9. Mistakes aren't bad. 10. Renewal and discovery needs to occur within the administrative agency as well as within industry. Do international regimes restrict IP? No - little of what's discussed here violates the IP, trade, or finance rules lodged in the WTO or other arrangements. The most harmful are explicit export subsidies, since exporting industries are known to develop faster and with better efficiency and technology than non-exporting ones. TRIPS is another. },
	Author = {Dani Rodrick},
	Date-Added = {2007-11-06 12:20:47 -0800},
	Date-Modified = {2007-11-06 12:21:53 -0800},
	Institution = {Harvard University, John F. Kennedy School of Government},
	Keywords = {zysman, ps201w11},
	Month = {September},
	Title = {Industrial Policy for the Twenty-First Century},
	Year = {2004}}

@book{Breznitz2006,
	Address = {New Haven},
	Annote = {Case study of the national strategies of IT competitiveness in Taiwan, Ireland, and Israel, founded on a basic question: all three states became hypercompetitive in international IT markets within one generation, but did so with very different success rates in different sectors. How to explain this? Supposition: that increased globalization in an increasingly complex economy gives states more, not less, flexibility in choosing national industrial policy. Because production chains are now disaggregated, states can try to enter them at a wider variety of points than ever before. However, there are caveats: unlike Gerschenkronian development, states cannot poach technology in the same way, given the pace of technological change. Rather, states need to make specific arrangements to bring technology in from outside and merge it with internally-developed technology. Modes of development: Taiwan: development of the pure-play IC foundry, insertion in the global production system of IC products Ireland: development of software and services, with little hardware, and heavy use of MNC and EU funds and assistance. Israel: success at niche hardware and software through explicit government sponsorship of R\&D Two theoretical problems: 1. How do states shape the development of cutting-edge industries like IT, where conventional wisdom holds that it is impossible to ``pick winners''? 2. Why did multiple different development pathways emerge? Problems with the Gerschenkronian view: 1. The markets these states entered were not already well-developed; therefore not merely a matter of marshalling resources to invest in known technology. 2. Catch-up is an oxymoron in an industry that is about the creation and application of new technology 3. Success models are diverse: none of these three states has bureaucracies, governments, or industries that look at all like each other. Conclusion: state-industry embeddedness in pursuit of rapid development is not a binary embedded / not embedded question. Rather, how and where the state embeds itself in the economy is critical to understanding the successes of the national development pathway. Thus, developmental bureaucracy NOT Weberian: not heavily insulated from industry. Rather, by choosing the way it is embedded with industry, it is choosing how it will both gain information from rapidly changing industrial conditions and then use that information to shape responses. Global production fragmentation: With production fragmenting by stage, it becomes possible to pick a stage to specialize in, aggregate the demand of different industries and companies in that stage, and gain economies of scale and thus competitive advantage. Terms this ``Production Stage Specialization'' Decisive factors in achieving this: 1. Relation of industry to global markets - in particular, the MNCs that ultimately design and source products 2. The development of specific capabilities for specific activities that are suitable to specific stages of production (Note: by inference, deciding on a specific stage of production has consequences for the demands placed on the state) Relation to MNCs is critical: depends on what portion of the production chain the MNC has outsourced to you: R\&D, intermediary design, intermediary assembly, etc. The demands of the various stages radically influence the set of skills and institutions you need to excel at that stage. New Model of State Industrial Policy: NOT ``picker of winners'' but instead facilitation for firms to ``make long-term commitments to operate in rapid innovation based industries and activities.'' Tension in this role: Between having enough information to play the role of facilitator / organizer, and not asserting all the power that would come with having such information to the extent that it imposes rigidities on the sector. LOTS of ways to do this: Taiwan: closed state, closed industry, but porous state-sponsored R\&D Israel: porous state and porous industry (lots of cross-population of experts and CEOs), linked through industrial development agencies Ireland: Closed state, porous industry, linked by porous development agencies. Key to recognize that this means changed functions for the state: Old state model: (1) facilitate deals for tech transfer with MNCs; (2) use control over the currency to shape industrial development through finance; (3) prevent MNCs from becoming too dominant at home and driving out local firms. New state model: Engage MNCs and global financial markets in different ways for specific sectoral purposes; develop indigenous R\&D rather than relying on tech transfer. Hence range of possibilities, from relying on MNC technology and becoming experts in production methods to relying on indigenous R\&D and becoming R\&D suppliers.},
	Author = {Daniel Breznitz},
	Date-Added = {2007-11-06 10:44:35 -0800},
	Date-Modified = {2007-11-06 10:45:20 -0800},
	Keywords = {zysman, ps201w11},
	Publisher = {Yale University Press},
	Title = {Innovation and the State},
	Year = {2006}}

@inbook{Nielsen2006,
	Address = {Stanford},
	Annote = {Basic point: knowledge management systems are about codified knowledge. Producing codified knowledge requires abstracting information from its context, in order to make it useful across contexts. But actually applying that codified knowledge to a specific situation requires that the people doing it have specific tacit knowledge about the situation, close familiarity with the codified knowledge, and a means of linking the two together. This interaction of ``tacit'' and ``explicit'' knowledge occurs through ``spoken-about'' knowledge within the firm. It is the process of interpreting codified knowledge in the context of tacit knowledge in order to tie generalized experience and information to a specific problem. The ability to do this within a firm presents the possibility for responding rapidly to changing demands from external clients. Technology allows the increased codification of knowledge. But that implies that it will also demand the increased application of ``spoken-about'' knowledge, with consequences for how firms structure themselves (and, implicitly, the demand sthey place on political economies to support those structures).},
	Author = {Niels Christian Nielsen and Maj Cecile Nielsen},
	Chapter = {12: Spoken-About Knowledge: Why it takes much more than knowledge management to manage knowledge},
	Date-Added = {2007-11-06 10:22:48 -0800},
	Date-Modified = {2007-11-06 10:23:58 -0800},
	Editor = {John Zysman and Abe Newman},
	Keywords = {zysman, ps201w11},
	Publisher = {Stanford University Press},
	Title = {How Revolutionary was the Digital Revolution? National Responses, Market Transitions, and Global Technology},
	Year = {2006}}

