	Pages = {127--144},
	Title = {Theories of the Firm: Contractual and Competence Perspectives},
	Volume = {3},
	Year = {1993},
	Bdsk-File-1 = {}}

@inbook{Brewster2006,
	Address = {Oxford},
	Annote = {Basic argument: we should not expect, and do not see empirically, a single form of firm organization within each VoC archetype economy. What we do see are trends - certain forms of firm coordination, financing, etc are more present in LMEs than CMEs. But within each, there are a diversity of solutions (small, large, etc) that operate effectively. Examples of diversity: -Collective bargaining and works councils not correlated across economies. One, the other, or both are present in many places. Examples of consistency: -LMEs seem to make greater use of layoffs and financial instruments than CME firms -CME firms much more likely to use some form of worker participation in the production design This is not to say that layoffs aren't present in CMEs, or worker participation in LMEs. But the predominance of their counterparts in both suggests trends deriving from firm-level responses to national-level institutions. },
