	Publisher = {Oxford University Press},
	Title = {{Beyond Continuity: Institutional Change in Advanced Political Economies}},
	Year = {2005},
	Bdsk-File-1 = {}}

@book{thelen2004iep,
	Annote = {Argument: skill development programs in the cases have their origins in settlements between skilled artisans, industrial workers, and employers during the early period of national industrial development. Those settlements were conditioned by government intervention, degree of worker organization, and firm prerogative. Those states that had no traditional artisanate (the US) or had destroyed it (the UK) did not found craft or skill training programs, and evolved into general skill economies. Those that had strong artisanal traditions (Germany and Japan) that were coupled to strong status indicators founded institutions that replicated these status levels and skill development programs, and evolved into specific skill economies. In general skill economies, the key tension was between workers and firms over the question of job creation and wage levels, leading to a high tendency towards defection (workers) and an unwillingness to support skill development (firms). In the latter case, the tension was between skilled artisans and industrial workers over pay and status, and developed in ways that encouraged skill formation and firm support of specific skill institutions. },
	Author = {K. Thelen},
	Date-Added = {2008-09-26 11:22:59 -0700},
	Date-Modified = {2008-09-26 11:23:36 -0700},
