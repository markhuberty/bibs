@article{Fairbanks1995,
	Annote = {Basic thesis: the role of the military in the post-Soviet states should be considered in a more nuanced fashion than it has been to date. War has been relatively successful - 11 or 12 of the ethnic wars have been won by the insurgent ethnicity. But coups have not. Standing armies have behaved well in some cases - as in a refusal to act against Yeltsin after the abortive coup attempt of 1990 - and not, as in the wars of Chechnya. Moreover, most of the former Soviet republics are remarkable for what hasn't happened - the rise of a military dictator, and in particular a dictator that sees the regional instability as an opportunity for empire building. With the civil infrastructure breaking down, the Western ideal of a civilian-directed military is difficult to achieve. So, too, when the prestige attached to being an officer has dropped, and the spoils available to corrupt men-at-arms risen. The role of military force should not be wholly denigrated, provided the security services can be encouraged to protect the rule of law against both their fellows and the corrupt civilian politicians who exercise nominal control over them. Thus careful steps should be taken when dreaming up solutions to the problem of militias and corrupt militaries. They are sufficiently embedded in the political and civic context, and sufficiently without non-criminal forms of prestige and compensation, that old admonishments about civil-military relations won't work. },
	Author = {Charles H. Fairbanks},
	Date-Added = {2007-05-21 09:08:23 -0400},
	Date-Modified = {2007-05-21 09:08:23 -0400},
	Journal = {Journal of Democracy},
	Number = {4},
	Pages = {18--34},
	Title = {The Postcommunist Wars},
	Volume = {6},
	Year = {1995}}

