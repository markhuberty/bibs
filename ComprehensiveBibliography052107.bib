@book{Constant1988,
	Address = {Cambridge},
	Annote = {Argument for divided government along the following lines: 1. A hereditary monarch with constitutional powers 2. A hereditary constituitive body along the lines of the House of Lords 3. An directly elected constituitive body 4. A citizenry (thus voting rights) based on land ownership (1) is needed to mediate among the other powers - the monarch's power cannot, per Constant, be vested in the other branches, because then the balancing of powers is inevitably a zero-sum game: certain functions gain only by the loss of others, and the delicate balance of powers is distorted. Far better a specific office that, while not engaged in the day-to-day of governance, can afford to take the longer view towards the protection of the general state of government. (1b) a set of appointed ministers with enumerated powers separate from the monarch, who are responsible for the administration of the laws but who can be recalled should they grow complacent or corrupt. (2) is necessary to support the monarch and to be the intermediate state between the monarch and the directly elected branches. Hereditary priviledges bring, in Constant's view, seriousness that mere lifetime appointment or longer elective terms do not. (3) is required to adequately capture the mood of the citizenry, to invest them with political power, to provide for domestic tranquility guaranteed by the regular exercise of political power, and to inject the concerns of the day as against the hereditary body's concerns of the era. (4) is demanded by what Constant believes is the special role of land ownership in cultivating the mind suitable to make wise choices in the polls. He rejects the idea that agricultural land and industrial property are equivalent, for industrial property haas none of the save virtues of permanence that he finds in agricultural cultivation. \_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_ Argument about the different liberties of the ancients and the moderns: The Ancients (mostly, Athens) gave up civil liberties to secure political liberties. They could do so because, in the context of a small polis where the franchise was held only be slaveowners with comparatively large amounts of free time, direct democracy could work only if each member of the polity had a clear picture of what all the others did. Hence civil liberties - basically, the right to a life separate from the community and private - were subordinated to the information requirement of political rights. This does not work for modern society. It is more diverse, larger, more complex, and faster-changing than ancient society. The goal of man in such a society is to carve out a private space through which to achieve his security, not to embrace the whole of society. He has neither the time, nor the resources, nor the close identification with his fellow man, to do so. Rather, the protection of civil rights is all, while his political right to direct democracy is diluted through representative democracy. Mistaking ancient democracy for modern was the root of the Terror.},
	Author = {Benjamin Constant},
	Date-Added = {2007-09-11 15:04:06 -0700},
	Date-Modified = {2007-11-14 08:44:32 -0800},
	Editor = {Biancamaria Fontina},
	Keywords = {fish, ps200w3, liberty, liberalism},
	Publisher = {Cambridge University Press},
	Series = {Cambridge Texts in the History of Political Thought},
	Title = {Political Writings},
	Year = {1988}}

@article{Zysman1994,
	Abstract = {Zysman calls this the ``market logic'' - thus in France, concerned about central control, entrepreneurship outside the bounds of state-mediated capital markets was very difficult, and so the 1980s see little development of small firm innovation; or in Japan, protectionism made economies of scale and rapid technological adoption from abroad critical, and so made the firms very fast to innovate and very likely to deploy massive capacity, preparing them for export markets. Questions: 1. Does Zysman still believe (2) in an era of very global supply chains? If it's now possible to source from and sell to almost anywhere, are national trajectories of supply / demand networks as important? 2. If the firm is the unit of innovation, how does national technology policy fit? What about extended R\&D networks? Is it, for instance, the poor university R\&D structure in Germany that leads to a paucity of small firms, or a lack of small firms that results in little commercializable technological innovaiton from German universities?},
	Annote = {Generally, an argument that both patterns of economic growth and technological development are rooted in historic political / economic institutionalism. But Zysman's a little vague on what comes first. Do political goals create institutions that constrain the possibilities for technological improvement (a la Dobbins on the railroads)? Or, with firms already active in the economy, do they create conditions on the ground to which politics responds? Zysman asserts three things: 1. The firm is the unit of technological innovation. That innovation develops path-dependency over time based on which innovations prove successful. Those that are, receive additional investment; those that aren't are quickly rejected. 2. The developing network of firms in an economy further constrains growth pathways by developing a set of supply and demand conditions that shape possibilities for future firm innovation. 3. Both (1) and (2) are rooted in a set of historic political and economic institutions that ultimately shape what the firm is able to do. },
	Author = {John Zysman},
	Date-Added = {2007-09-11 11:18:57 -0700},
	Date-Modified = {2009-08-05 08:55:28 -0700},
	Journal = {Industrial and Corporate Change},
	Keywords = {zysman, ps201w3, institutionalism, corporate development, technology, growth},
	Number = {1},
	Title = {How Institutions Create Historically Rooted Trajectories of Growth},
	Type = {Working Paper},
	Volume = {3},
	Year = {1994}}

@book{Bendix1964,
	Address = {New York},
	Annote = {pp48-54, on Tocqueville's analysis of the effects of the French Revolution in ``L'Ancien Regime et la Revolution'' Tocqueville was first to point out that the revolution had destroyed an existing social order that, if unjust, nevertheless made inequality acceptable. In its place, it had set up a two-tiered system of rights. Politically, everyone was to be the same under the law. But economically, the old regime retained much of its wealth, and the former servants by custom were now servants by employment. And so they saw their political and economic inequality as incommensurate. Under such a system, Tocqueville worried about whether it was possible to preserve freedom. Free individuals, the bonds between them destroyed by the revolution, would have few options for amelioration of their situation except to turn to the government. A government empowered by the dependency of the citizen on the state could as easily abuse freedom as it could protect it. America avoids this problem through the strength of its social institutions. This provides a society in which man is equal to all and can observe it. In his working life, there is no shame or feeling of inferiority in entering into contract to perform labor for another. (Note uniqueness to France: concern was ordering. First the feudal system, with reciprocal rights and responsibilities, exists; then the ancien regime destroys this via centralization of power in the king - which leads to the destruction of reciprocity as the middle classes compete for favoritism while exploiting the peasants. Thus when the revolution comes, the classes all look on each other with hatred and suspicion, and are unable to reconstitute a social order in which they can be equal. Since this is then combined with economic inequality, people look to the state to solve economic inequality as a means of correcting the social inequality. And the state's power again grows at the center.)},
	Author = {Reinhard Bendix},
	Date-Added = {2007-09-11 09:14:24 -0700},
	Date-Modified = {2007-09-11 09:22:03 -0700},
	Keywords = {zysman, ps201w3, tocqueville, freedom, french revolution},
	Publisher = {Wiley},
	Title = {Nation-Building and Citizenship},
	Year = {1964}}

@book{Pierson2004,
	Address = {Princeton},
	Annote = {Introduction. Pierson's classic work on historic institutionalism. An argument that political development can be explained in large part by the pattern of establishment and survival of institutions over time. In this pattern, two things are important: 1. Sequencing (the order in which events take place is critical to the outcome of those events) 2. Path-dependency (institutions tend to reinforce themselves by shaping decisions and outcomes in a way that, over time, generates increasing returns and makes costs of changing more expensive.) Note that Pierson doesn't deny the importance of, say, rat choice methods in evaluating certain outcomes. He does, however, note that their inability to account for historic circumstance or ordering, due their tendency to take mere snapshots of a particular political process, leaves them wanting for better explanation. His attitude toward functionalism is the same: some institutions cannot be explained purely by the functions they perform, particularly if those functions could be performed more efficiently by a different institution. The history matters - both the circumstances of the institution's founding and the institution's precursors. },
	Author = {Paul Pierson},
	Date-Added = {2007-09-11 08:36:32 -0700},
	Date-Modified = {2007-09-11 08:46:56 -0700},
	Keywords = {zysman, ps201w3, historic institutionalism},
	Publisher = {Princeton University Press},
	Title = {Politics in Time: History, Instituions, and Social Analysis},
	Year = {2004}}

@article{Hall1996,
	Annote = {1. Historic Institutionalism -Institutions as the principal factor structuring behavior and shaping outcomes -Institutions are broadly defined -Institutions embody power asymmetries -Institutions generate path dependence and unintended consequences from initial actions In brief, institutions alter expectations about the future, and thus alter decisions made now that take those expectations as input. Approaches to understanding this include both a calculus approach - providing more or less certainty about the future from which actors calculate their best action now - and a cultural approach - that institutions shape the culture in which decisions are made and in so doing invisibly constrain the list of choices considered at any point in time. 2. Rat Choice Institutionalism Origins: studies of the American Congress; responding to theoretically unexpected stability of Congressional outcomes. Answer was to realize that the institution of Congress shaped outcomes by lowering transaction costs associated with certain paths of action. Four features: -Rat Choice behavioral assumptions (utility-maximizing, aim for largest majorities possible) -See politics as a set of collective action dilemmas -Emphasize strategic interaction (calculus approach where inputs are expected behaviors of others in process of pursuing given outcome) -Institutions exist because they provide some value in the functions they carry out for the primary political actors 3. Social Institutionalism (See Dobbins) That institutions originate from culture and cannot be understood as spontaneously generated objects. Three unique features: -Institutions are defined more broadly than in the other two types: ``frames of meaning'' guiding human action -Unique understanding of the mechanism through which institutions act: more ``cultural'' than ``calculus'' in the sense of defining a set of perceptions of the world and thus shaping the possible choices made in response to perceived problems or opportunities. ``rational action'' is socially constituted. Strengths and Weaknesses: -Rat Choice: weak at explaining why institutions arise in the first place, usually because it backs into causes from effects (e.g., enough people needed institution X, and so it came into being), whereas often this isn't really sufficient. But generally good at addressing why people behave in certain ways within already-existing institutional settings. -Sociological: better at explaining the genesis of institutions, often by noticing similarities between new institutions and old ones - consistent with the idea that existing institutions shape perceptions. But weak at addressing situations of institutional conflict (e.g., where people within the same institution have different ideas) -Historical: Good at showing how path-dependency works to reinforce institutions over time. Bad at generalizations, because of their reliance on the historical record of the particular area of study. },
	Author = {Peter A. Hall and Rosemary C.R. Taylor},
	Date-Added = {2007-09-10 22:22:17 -0700},
	Date-Modified = {2007-09-11 08:16:56 -0700},
	Journal = {Political Studies},
	Keywords = {zysman, ps201w3, institutionalism},
	Pages = {936--957},
	Title = {Political Science and the Three New Institutionalisms},
	Volume = {XLIV},
	Year = {1996},
	Bdsk-File-1 = {}}

@book{Dobbins1994,
	Address = {Cambridge},
	Annote = {Introduction and Conclusion Summary: that structures for managing the economy evolved alongside structures for managing the state, and replicated many of the stat's characteristics; and that those structures not only gave certain people the power to perpetuate the form of railroad regulation, but also shaped their perceptions when doing so, such that future moves to regulate new industries tended to draw on experience in regulating the old, such that the pattern of state regulation in old industries was replicated in new ones. Basic question: why, if, as economists argue, economic laws are universal, do we see such diversity in national economic systems? What if economic laws are not universal in their demands on economic organization? What if, instead, they offer a diverse range of possible outcomes. Then what is the cause of this diversity? Dobbins argues that it is a ``national policy logic'' centered on national culture born of a particular story of national political issues at the point of industrial birth. Thus Britain - born of a political system that emphasized individual autonomy - developed a regulatory system that gave power to small firms and resisted direct involvement in railroad planning and financing. Later government activism was directed at protection of small firms, even via allowing cartelization. France, which struggled constantly with asserting state control over a large and unruly territory, put substantial power in the hands of state bureaucrats to plan and regulate the railways as a means of tying the nation together. The US, with a federal system, left regulation to the states until such time as concentration of power in the hands of a few threatened to weaken federalism, at which point it intervened. Or, as the author says, ``the political structures used to achieve national unity and military glory were soon retrofitted to the need to promote economic development.'' Changed ``Perception'' and ``Selection'': Perception of the problem (e.g. is it a problem of too much industrial concentration, too little, the wrong kind, etc) and selection of the solution. cf. Gerschenkron: Dobbins argues that the ``sequencing'' argument doesn't hold in absolute terms: the US under this analysis should be expected to be the most interventionist of the three states, but it was not. (Though this may be a flawed reading of Gerschenkron - argument is about when and where you industrialize, in relative terms, not absolute. },
	Author = {Frank Dobbins},
	Date-Added = {2007-09-10 22:02:06 -0700},
	Date-Modified = {2007-09-10 22:19:35 -0700},
	Publisher = {Cambridge University Press},
	Title = {Forging Industrial Policy: The United States, Britain, and France in the railway ageq},
	Year = {1994}}

@book{Berger1972,
	Address = {Cambridge},
	Annote = {Question: how to explain different levels of state-municipality relations (in the sense of activity in national politics) in different departments in France? Answer: the strength of corporatist institutions tied to agriculture were the determining factor. In areas where they were strong (Finistere, in her study), Party affiliation and party performance at the national level meant relatively little, since the corporatist institution managed most of the state-society relations. In areas where corporatist groups commanded little power, party affiliation and performance meant a great deal, since local outcomes were influenced much more by national actions. Hence the political outcomes seen at the local level: in Finistere, you see consistent party affiliation and relatively low rates of turnover in elected officials, since little was expected of them. In Cote du Nord, the opposite is true, and regular turnover is seen as officials fail to deliver on promises aand are removed in favor of others. Furthermore, since turnover is more rapid in non-corporatist settings, more recent issues tend to be more decisive in elections. Nor can other effects account for this. Income, sectoral distribution, demographics, religion, demographic change (rural exodus), and local political history don't differ much. Where they do - as in local political history - they would be expected to divide each locality internally, rather than present huge differences between the two. Why does organizational membership shape behavior: because it constrains options. If you belong to a corporatist group, it is difficult to appeal to the national government for redress of grievance, as the corporatists have local control over the resources needed for redress. Additionally, the corporatist group may have ultimate choice in candidates for election, thus restricting electoral options for political participation. Note that parties fulfill the same role (selecting candidates and controlling access) but don't do so at the same level.},
	Author = {Suzanne Berger},
	Date-Added = {2007-09-10 21:39:04 -0700},
	Date-Modified = {2007-09-10 21:40:11 -0700},
	Keywords = {zysman, ps201w3},
	Publisher = {Harvard University Press},
	Title = {Peasants Against Politics},
	Year = {1972}}

@inbook{Durkheim1993,
	Address = {New York},
	Annote = {Distinguishes between two types of individualism: negative and social. Negative is that of Berlin's ``negative liberty'' in the sense of the individual being able to decide freely for himself how to live, within certain limited societal constraints. Durkheim's is social in that it anticipates that the common humanity of man is the only thing binding together large societies, and that this common humanity is tied to the exercise of reason. That reason is based on knowledge - either of the subject at hand, or of one's ignorance of that subject that should lead one to trust others who are demonstrably more knowledgable. In any case, the attack on the individual is seen as an attack on the exercise of reason, and therefore on common humanity, and thus on the cohesion of society. In this sense, defense of individualism is not an alternative to the defense of society - it is the defense of society. Note that Durkheim isn't compatible with Berlin: he assumes that the individual does not really exist in isolation, since what is important is the common humanity expressed through the exercise of informed reason; nor is political freedom an end, as in Berlin - it is a means to a better, more reasonably society. If not used in such a way, it is, he says, dangerous. (Rationalist view - there is reason, we use it to perceive the world based on knowledge, that knowledge suggests demonstrably correct and incorrect outcomes, thus the need to defer lest one be mistaken, thus one basic ``correct'' world based on the ``correct'' judgements of all those with knowledge?},
	Author = {Emile Durkheim},
	Chapter = {Individualism and the Intellectuals},
	Date-Added = {2007-09-09 15:36:01 -0700},
	Date-Modified = {2007-09-09 16:05:21 -0700},
	Editor = {James Farganis},
	Keywords = {fish, ps200w2, individualism},
	Pages = {88--96},
	Publisher = {McGraw Hill},
	Title = {Readings in Social History},
	Year = {1993}}

@book{Berlin1992,
	Address = {Oxford},
	Annote = {General argument: that, at root, two kinds of liberty are asserted: negative liberty (the restriction of the scope of powers of coercion exercised by the society and state, with the goal of maximizing the realm of life free of state interference) and positive liberty (the goal of relieving individuals of the burdens or inequities that characterize their lives, under the assumption that it is possible to use the state to improve individuals' lives rather than merely offer equal protection under a limited set of laws.) Berlin is a natural ally of negative liberty, and views the positive kind with caution - potentially useful but also quite dangerous. The ultimate end can be, paradoxically, the curtailment of freedom to achieve freedom. Mill as originator of this idea: civilization advances through the search for truth; truth can only emerge through free inquiry, which cannot by definition occur in a society based on compulsion. Nevertheless, this statement has a problem: can truth not be sought by individuals living under oppressive regimes (cf. Eastern Europe, postwar period). Truth has been sought for a very long time, though real political liberty is a comparatively recent occurance. Despite this, Mill has a critical point: liberty in the sense of freedom from coercion doesn't care so much about who controls as how much control is alloted to the controller. Parliaments may be as coercive as autocrats. Compare with positive freedom, which Berlin defines as ``freedom to lead one prescribed form of life.'' Berlin claims it derives originally from a reasonable idea: that one wants to live by ones own choices, not external forces of any kind. Berlin argues that this isn't too different from negative freedom, but through a series of logical leaps, becomes so. How? Through the separation of the self into two: ``higher'' and ``lower'' or ``selfish'' and ``communal'' or the like. It is assumed that leading the lesser of the two is inevitably a sign of having developed under those ``external forces.'' Thus the state, in educating the individual and attacking those external forces, will aid the individual to achieve his ``true self'' or ``better self'' and thus achieve the ``positive'' freedom he so desires. (Note the problem here is the leap from the individual making this choice to the society - and thus, implicitly, the idea that there is a universal form of the ``higher'' self that must be imposed; no a priori reason to believe this). What does this imply? First, that someone, presumably not the individual, can see the two selves, and second, that they should have the power to do what is necessary to help the individual see it in themself. In one form, this takes the form of asceticism - self-denial in an attempt to reach a higher state. Fine if this is motivated by the individual. But in the limit, if that self-denial is the mental discipline to ``free'' one's self amidst the most oppressive dictatorship by convincing the self that it really doesn't need self-determination, then it in the limit is nonproductive of freedom. See Kant: values originate from free human choice; choice implies the liberty to choose; thus restriction on choice - coercion - in the name of some ``higher'' value is a fallacy, because choice is the highest value of all, from which all other legitimate values proceed. Rationalism is another version of this: it assumes not a higher and lower self, but a rational and irrational self. Various forms of coercion - including education, potentially - are used to ensure that the individual realizes and chooses the rational course of conduct (see not only the Benthamites, but also the Idealists). ``Positive doctrine of liberation by reason.'' Moreover, since rationalism presupposes the internal consistency of everything, this implies only one set of truths - and therefore, one kind of life. Finally, since rational beings want to live in a rational world, they can only achieve this by making the world - and everyone in it - rational; in other words, they must become dictators lest they be condemned to a life where the rationality they can see isn't achieved by their fellows. And so follows dictatorship. Status is another form of positive liberty. If I believe that, since I am not recognized as anything other than an individual, that my true human nature is being suppressed, then I need to rectify this situation by forcing the society to grant me higher status by recognizing my unique characteristics. Implies that my fundamental identity is social, not individual: that I do not exist separate from the group. But the guarantee of status has little relation to guarantees of negative liberty: thus I may be acknowledged for what group I belong to while nonetheless living under the most oppressive of regimes. (Here Berlin invokes the preference of some colonial peoples to be governed despotically by their own rather than less despotically by their colonizer.) Finally, Berlin notes that most revolutions - even the most collective and oppressive - are usually couched in terms of liberation from oppression - and thus ``negative'' liberty. That they can do so suggests a fundamental misconception of where liberty stems from, and where its logic leads. From Mill / Constant / Tocqueville, the preconditions for a free society: 1. No absolute power, only absolute rights 2. Authentic frontiers within which individuals are inviolable, based on long-held rules that make up the conception of what constitutes a normal human being - or, as Berlin says, rules that a person could not violate without revulsion. Where does the impetus to act contrary to these principles arise: the notion that there is one, final, pure, uncorruptable ``solution'' that will yield a utopian life. But, as Berlin notes in his favorite phrase, ``out of the crooked timber of humanity, no straight thing was ever made.'' Note also - ties to his definition of ``tolerance'' as being able to, within the limits prescribed by negative freedoms, envision that it is possible to live another kind of life than the one you live, to deem that acceptable, and yet still to defend your own life as valid. Tolerance is understanding within a framework of negative freedoms.},
	Author = {Isaiah Berlin},
	Date-Added = {2007-09-09 14:59:58 -0700},
	Date-Modified = {2008-09-19 15:28:43 -0700},
	Keywords = {fish, ps200w2, positive liberty, negative liberty},
	Local-Url = {file://localhost/Users/markhuberty/Documents/BibtexAutofilePapers/Berlin/1992.ris},
	Publisher = {Oxford University Press},
	Title = {Four Essays on Liberty},
	Year = {1992},
	Bdsk-File-1 = {}}

@book{Spencer1994,
	Address = {Cambridge},
	Annote = {1. The New Toryism That the salient difference between the original Liberals and the Tories was the attitude towards the state's power of coercion. The Liberals attempted to restrict that power thoughout areas of private life, while the Tories either attempted to preserve it or expand it. Now, however, the Liberals, in the guise of ameliorating various social ills through supposed ``responsible'' governance, have begun expanding the power of the state and its scope in private life. The result is a narrowing of the political gap separating the two parties: in principle they are the same, though in aims they remain different. 2. The Coming Slavery That the expansion of the state's power of coercion will, inevitably, result in slavery, defined as the ownership of one's decisions in life by another. Spencer argues this is true because early, marginal expansions of state power into civil life do several things: first, they have unintended consequences, often negative, which then require further state intervention to solve but almost never produce a liquidiation of the first act; second, because they validate the idea that only the state has the power to act in amelioration of certain ills, and so inculturate in the citizen the behavior of turning to the state; and third, because the logical end of these expansions isn't the particular goal in mind for the initial act, but the relief of supposedly preventable human suffering - a goal that, if validated, means an attempt to regulate all sources of life that produce such suffering, and thus all life itself, for human life is ultimately flawed. (Note neo-functionalist character of this: once the state is in one part of life, it finds it necessary to intrude into another; critique of the EU) 3. The Great Political Superstition That Parliament is not sovereign, as it does not presume to derive its power from God, but rather from the people, and the people, having many areas of private life on which they would not consent to obey the will of the majority, thus cannot have intended that the majority, as represented by Parliament, would govern all of life under the principle of majority rule. Rather, Parliament's purview is limited by the natural rights, which are few and which can be both philosophically and empirically arrived at: they govern the natural need of the individual to provide for himself the things necessary to sustain life, and to have those things protected; this implies at root a protection of life and property against the aggression of others, protections Spencer argues exist even in countries without formal legal systems. Parliament thus has a duty to protect the individual, both as a solitary figure and when he enters into voluntary contract with others. Beyond these protections, the power of Parliament is severely limited. (Direct attack on Benthamite ideas of governance: the problem is not a cost-benefit analysis between the immediate costs of some program and its immediate benefits, but the long-term effect of such programs on the overall health of the society and the society's ability to stay both prosperous and free. It is, for Spencer, the failure to observe this long-term problem, that is to blame for the expansion of parliamentary power without much outcry from the public, and, in line with (1) and (2), destined to end in the destruction of the free society.) Ancestor of Hayek...},
	Author = {Herbert Spencer},
	Date-Added = {2007-09-09 14:47:40 -0700},
	Date-Modified = {2007-09-09 15:00:27 -0700},
	Keywords = {fish, ps200w2, liberty, natural right},
	Publisher = {Cambridge University Press},
	Title = {Man Versus the State},
	Year = {1994}}

