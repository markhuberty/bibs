@article{Bordo:1991,
	Abstract = {CONCLUSION While the Napoleonic wars after 1797 offered the curious spectacle of faithful Albion abandoning the gold standard and perfidious France maintaining convertibility of the franc, these war finance regimes were the consequence of each nation's credibility as a debtor. Given its long record of fiscal probity, coupled with its open budgetary process in Parliament, Great Britain could continue to borrow a substantial frac- tion of its war expenditures at what were relatively low interest rates. British tax rates did not vary much over most of the eighteenth century as peacetime surpluses offset wartime deficits to pay off the accumu- lated war debts. Taxes would not have been greatly increased during the Napoleonic wars except that their duration imposed a debt burden much higher than the eighteenth-century norm, requiring a rise in the tax rate to sustain the nation's credibility as a borrower. France, on the other hand, had squandered her reputation in the last decade of the ancien re'gime and the Revolution. Her dependency on taxation did not reflect any superior fiscal virtues but rather the opposite. Borrowing would have been exceedingly costly and the public was skeptical of the empire's fidelity. Inherited credibility resolves this paradoxical pairing of fiscal regimes. },
	Annote = {Good argument about how tax system credibility transmutes into financing capability. The British, with a more advanced financial system and greater governmental accountability, could borrow more at lower rates and abandon gold because of greater reliability of repayment. The French could not, because of past experiences of non-repayment, deficits in peacetime, and failed commitments to fiscal prudence. Transparency plays a big role. The post-1688 dominance of Parliament was important in securing the transparency of British finances, which meant that hte public and the financial markets would regularly exact penalities on a state that practiced fiscal imprudence. Hence the pressure on Pitt to repeatedly reform and strengthen British finances, and their very good track record at repayment after the wars of the 18th century. France, despite attempts to make similar commitments, was unable to honor them with any reliability. Question: where are the firms? What did business do? How large an influence did business have?},
	Author = {Michael D. Bordo and Eugene N. White},
	Date-Added = {2008-03-07 16:49:32 -0800},
	Date-Modified = {2008-03-09 13:26:48 -0700},
	Journal = {The Journal of Economic History},
	Keywords = {finance, economic history, britain, france},
	Local-Url = {file://localhost/Users/markhuberty/Documents/BibtexAutofilePapers/Bordo/1991.pdf},
	Month = {June},
	Number = {2},
	Pages = {303--316},
	Title = {A Tale of Two Currencies: British and French Finance During the Napoleonic Wars},
	Volume = {51},
	Year = {1991},
	Bdsk-File-1 = {}}

