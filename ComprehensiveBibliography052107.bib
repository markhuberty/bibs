@inbook{Pedersen:1993,
	Address = {Cambridge},
	Annote = {General: that TH Marshall's view of the welfare state as derived from evolving ideas about social citizenship, while useful, ignores the role that redistribution plays in dealing with the development and maintenance of families. Historical evidence clearly shows that this question was in the minds of those arguing over the welfare state - whether children and wives were a social good that society should support, or a private choice that was thus left to the individual. Moreover, this formulation usually treated women as dependents in law as well as fact, a situation that has persisted with some ill effects, particularly in Christian Democratic states. Thus the need to reconsider welfare policy in light of debates on family. Like Esping-Andersen, three areas of redistributive policy: the welfare state, the labor market, and family policy. France and Britain provide the cases: Britain as a case in which family distribution was based heavily on the male breadwinner model, and was meager in benefits; France as a case of social citizenship with rich benefits. Why the difference? Pedersen argues that the answer lies in two debates: the political debate over the structure of benefits, and the economic considerations of various policy options. The political rhetoric was key. In Britain, the rhetoric was dominated by those who either wanted to protect existing benefits or guard against the gender redistribution aims of feminists (the civil service and the trade unions, respectively). In France, the shaping of debate as one over patriotic duty to restore the population after WWI won the day. The economic argument: that the move towards family-level distribution reflects the relative strength of the British TUC and the French capital owners. The TUC resisted family distribution out of concern that it would restrain the growth of men's wages; the French capital owners supported it on the same grounds. That both won suggests the relative power of labor in Britain versus France.},
	Author = {Susan Pedersen},
	Chapter = {Introduction, Conclusion},
	Date-Added = {2008-09-15 20:36:13 -0700},
	Date-Modified = {2008-09-15 20:47:14 -0700},
	Keywords = {origin of welfare state, levy, family dependence},
	Publisher = {Cambridge University Press},
	Title = {Family, Dependence, and the Origins of the Welfare State},
	Year = {1993}}

@article{Huber:1993,
	Annote = {GENERAL: questionable estimation strategy. Why do we expect these things to be linear? What tests were done to determine if the data was poolable across either country or year? Tests for multicolinearity (and thus artificial inflation of the coefficients) were done, and some more egregious correlations eliminated. Pooling by country gives the time-series results; pooling by time period gives the x-sectional results. A few other problems: 1. If there's a time trend in the data (as there almost certainly is in, say, the GDPPC numbers), then the GLS methods are incapable of dealing with them. GLS assumes an error structure of the covariates in the time-series results that is the same for all time (e.g., in each time period, the error structure is the same). But if there is any autocorrelation behavior taking place, this is non-sensical, because the errors will be different at different points in time. The authors do make some attempts to deal with the non-standard errors, but make no explanation for why the 1a. Unless you believe that the unit root processes that could be taking place in both are equivalent (common unit root), then the errors will still be wrong (probably too large). 2. Why isn't GDPPC corrected for real figures? Why is CPI treated differently? CPI shouldn't matter to the relative measure of the welfare state size (since, to a first approximation, the ratio of SS/GDP has the CPI in both numerator and denominator); unless you have some reason to believe that the inflators for the two figures are different, and even then this doesn't help you. Including CPI in this fashion doesn't take care of the inflation process buried in the GDPPC number, either. It would if they were using logs, but they aren't. 3. Reasoning is weird. They get a negative correlation between strike rate and social spending and argue, among other things, that it is "incorrectly signed." What's that supposed to mean? That it doesn't fit the priors? What they conclude: 1. Christian democracy is better correlated with welfare state founding than social democracy 2. Social democracy is better correlated with welfare state expansion that CD 3. Odd conclusion: "It is less rising affluence over time than it is being in the club of richer countries." Doesn't "rising affluence over time" put you in the club of rich countries eventually?},
	Author = {Evelyne Huber and Charles Ragin and John D. Stephens},
	Date-Added = {2008-09-15 18:12:26 -0700},
	Date-Modified = {2008-09-15 18:50:12 -0700},
	Journal = {American Journal of Sociology},
	Keywords = {origin of welfare state, levy},
	Number = {3},
	Pages = {711--749},
	Title = {Social Democracy, Christian Democracy, Constitutional Structure, and the Welfare State},
	Volume = {99},
	Year = {1993}}

@inbook{Immergut:1992,
	Address = {Cambridge},
	Annote = {Question: how can institutionalism account for both continuity and change? Answer: institutionalism is not fundamentally about how a set of rules per se persist, but about how those persistant rules shape the way that policy inputs are translated into outputs. Thus a changing set of demands from the input side can still cause changed outputs without the institutions, properly understood, changing. Cases: Why do some nations have national healthcare systems and others do not? In Switzerland, France, and Sweden, national systems were proposed. The Swiss rejected it wholesale, the French settled for a mixed system of public insurance and private doctors, and the Swedes for a fully nationalized system. How to explain this institutional variance? By looking at the institutions that existed when thesse policies were proposed, and how they translated the responses of various social actors into policy. Immergut argues that the distance between the three policy proposals was minimal, but the outcomes widely different. The difference appears to hinge on the political institutions of the three countries. Rejected explanations: 1. Doctors' opposition: in each country, the doctors opposed it. Why did they get their way in Switz. but not in Sweden? Why weren't Swedish doctors, in shorter supply than their Swiss counterparts, even stronger than the Swiss? 2. Union preferences. Swedish unions were stronger, numbers-wise, than French; but the Swiss were also stronger than the French 3. Political partisanship - while the Swedes do have stronger socialist parties (and the other countries line up appropriately), that strength doesn't correlate well with preference on this particular issue. 4. Role of the bureaucracy: but the bureaucracy remains relatively fixed even as policy changes. Moreover, these structures (such as Swiss federalism) don't appear to have played a role in dictating political outcomes. Instead: The structure and location of veto points shaped how social actors took preferences, conceived outside the institutional structure, and put them into practice. This is contra other theories of institutionalism (from Hall, Weir, and others) that suggest that institutions constrain policy preferences ahead of implementation. Rather, in each case the social actors' preferences had much in common (among doctors vs. among labor) but three different outcomes resulted, consequence of the different institutional conditions under which they operated.},
	Author = {Ellen M. Immergut},
	Chapter = {3: The rules of the game: the logic of health policy-making in France, Switzerland, and Germany},
	Date-Added = {2008-09-15 17:54:10 -0700},
	Date-Modified = {2008-09-15 17:55:56 -0700},
	Editor = {Sven Steinmo and Kathleen Thelen and Frank Longstreth},
	Keywords = {levy, health policy, origin of welfare state, institutionalism},
	Publisher = {Cambridge University Press},
	Title = {Structuring Politics: Historical institutionalism in comparative perspective},
	Year = {1992}}

@inbook{Baldwin:1990,
	Address = {Cambridge},
	Annote = {INTRODUCTION: 1. The welfare state is a risk-sharing mechanism more than a redistributive mechanism 2. Social risks were insured based on some agreed-upon degree of social fairness across classes 3. How do we explain the differences in social solidarity, and its causal role in determining the extent of social insurance, across countries? Clearly some are different than others - Scandinavia vs. Britain, Germany vs. France, etc. At root, this is a question of how the societies in question arrived at an agreement on risk-sharing. 4. The evolution in different social groups' abilities to manage risk internally account for some of the reasons for welfare state variability and evolution over time. 5. The existence of hte social insurance system itself changes the conditions under which groups perceive and attempt to alter their abilitties to manage social risk But how to explain solidarity? Two possiblilities, both insufficient: 1. Economics - solidarity as self-interest in another guise, on the basis that for some things, you can only get goods through cooperation. But if that is true, why is their social conflict over certain kinds of solidarity (e.g., redistributive pension schemes?) 2. Altruism - solidarity as a natural human trait; but why then is it only active under certain conditions? Two more possibilites, better but still lacking: 1. "Laborist" approach - the solidaristic social welfare state is the result of certain groups attaining political power; the extent of the solidaristic state is a function of how significant and durable that power was. (But if true, how to explain welfare states emergent from other situations?) 2. The "common experiences" approach - that groups that had traditionally not been very interested in helping each other were, through common experiences like WWII, brought together in post-experience attempts to create a more just society. (But if true, then do welfare states only emerge out of catastrophic events that reshape social relations?) Interpretations for WS Origins: 1. Bonapartist: Politically functional: how do welfare states help to preserve the status quo? Quasi-Marxist, insofar as it argues that the WS runs counter to the long-term interests of the working class. But the question remains, why: either WS concessions were unavoidable concessions to the needy to avoid revolution; or the elites that did not undertake them failed to recognize clear self-interest. Why? 2. Social: Similar to a power-resources argument about the ability of the working class to accomplish class interests via the pursuit and attainment of political power. But - how then to account for the significant role played by corporatist groups (representing non-working class interests), Catholic parties (typically more bourgeois), and other non-working-class actors who contributed to the formation of the welfare state? 3. State arguments Note that both earlier arguments assume direct translation of intentions of "founding elements" into law and practice. But the state - as an administrative body with a role to play in putting law into practice - is a separate actor here. Thus interests may not be translated 1:1 into outcomes. But - Baldwin notes that this replaces one ambiguity (which social actor is most important) for another (how independent is the state from the social actors?). The role of the state is important - but it is also constrained by the positions of the social actors, particularly in democratic or quasi-democratic societies. Baldwin's interest: Those debates over the universal benefits (pensions, health insurance) rather than benefits targeted at specific groups. How does the ultimate extent of universality (how universal is universal) express the solidarity of a society? CONCLUSION: 1. The laborist interpretation fails - because the interests in social solidarity evolve over time. While the bourgeois was more self-reliant in the immediate postwar period, economic changes by the 1970s had changed its relations to the state, in ways that made it more interested in the solidaristic benefits that the welfare state could offer. 2. The existence of the welfare state itself changed the environment in which people assessed their interests. It became reasonable to consider entering a broader risk pool once that risk pool already existed; thus there is an element of historical institutionalism to the welfare state, in the sense that it changes the cost-benefit calculatoin of groups outside of it, possibly leading to expansions in the terms of solidarity. 3. Solidarity only emerges, in the risk-sharing sense, when different groups in society see themselves subject to the same risks. Tracking how that evolution of risk exposure takes place gives insight into how the social groups are created that sponsor and sustain the welfare state. 4. },
	Author = {Peter Baldwin},
	Chapter = {Introduction: Welfare, Redistribution, and Solidarity},
	Date-Added = {2008-09-14 14:18:36 -0700},
	Date-Modified = {2008-09-14 14:22:07 -0700},
	Editor = {Peter Baldwin},
	Keywords = {origin of welfare state, levy},
	Publisher = {Cambridge University Press},
	Title = {The Politics of Social Solidarity: Class Bases of the European Welfare State 1875-1975},
	Year = {1990}}

@inbook{Esping-Andersen:1984,
	Address = {Oxford},
	Annote = {Power-resources approach to the origins of the welfare state: the boundaries of the state were shaped by a multitude of factors, important among them the character of the power distribution among social actors. Important variables: the degree of unionization; the degree of working-class political organization (these two most important for the authors); budgetary constraints; ownership of capital; labor power and the number of wage earners vs. other forms of income dependency. This approach is contra the approach of the analysts who look only at quantitative variables of the welfare state - notably size of WS budgets. This doesn't necessarily explain what the state really does, or why it does it. Large pension policies in the absence of other WS programs suggest a particular type of politics. Assessment of PR for the three cases: Sweden as the strongest case, a function of the very strong LO labor union, its close association to the social democratic party (SAP), and the unionization of salaried employees under the TCO. Austria as the middle case: The strength of the labor unions and their ties to party approximate those of Sweden, but are weakened by internal religious and political cleavages that dilute their power to act as a unified body, a problem that does not exist for the bourgeois under the OVP. Germany as the weakest: the labor unions have a relatively small share of the wage-earning population; salaried workers are not unionized; and the ties to the SPD, while strong, have not propelled the SPD into power (SPD in opposition from 1945-1966). Purpose of the WS: De-commodification: the creation of circumstances under which a reasonable standard of living can be enjoyed by families regardless of marketplace success; the divorcing of social existence from market performance. "Social democratic" WS as those that most fully satisfy this criteria - the WS as an integral part of politics; vs. the "bourgeois" state, in which the WS is marginal to politics and supplies a correspondingly more marginal set of benefits. Politics of the WS: -Labor/left have sought social democratic institutions that aggregate the largest portion of the populace as possible into common welfare state institutions, cutting across class lines, in order to institutionalize the distribution of resources and promote social solidarity. -Bourgeois parties have sought WS institutions that are more minimal and, where possible, preserve the existing set of social status separations (e.g., Germany, male breadwinner-centric welfare state and benefits proportional to wages). How the cases fit: -Germany, very strong bourgeois (esp. Catholic/Christian-democratic), and a weak pre-war labor movement, lead to a bourgeois welfare state. -Scandinavia, weaker bourgeois and stronger labor movement, lead to a social-democratic welfare state. Sweden: The dual presence of urban workers' parties and farmers' parties made a red-green coalition possible starting in the late 19th century, and weakened the bourgeois by denying it an ally in the working class. The structure of the welfare state was consciously designed to be as inclusive as possible, so to induce social solidarity. Postwar reform and extension continued this process. The 1959 pension reform pushed social policy into the domain of housing, crowding out the private housing market. Problems emerged with the 1970s and the conflict between the guaranteed wage policies and the mandate for full employment and high productivity. The Lib/Cons governments of 1978-1982 have attempted to push back on these policies, via welfare state retrenchment policies, but made little headway prior to the return of hte social democrats. Austria: An explicitly corporatist strategy in place before 1914 prevented the emergence of working class solidarity. -Early WS policy was adminsitered by Conservative governments; but as of 1900, this had largely failed to prevent the emergence of social democratic / socialist parties, and so was abandoned as a strategy. -Social competition in the 1920s drew on the older corporatist patterns: liberal and conservative parties made sure that benefits were structured and packaged to deal with particular social groups, rather than cross-class groups. This pattern continued after 1945, until the election of an SPO (social-democratic) government in 1970, at which point efforts were made at social welfare reform to decrease the degree of regressivity. -Nevertheless, the structure of the welfare state remains generous but highly stratified. Austria has achieved this balance with a set of social programs that deliver, but in a market that tolerates higher degrees of income inequality than other states. Germany: -Postwar, unions were weak except in specific sectors; the DGB did not include salaried or farm employees. The CDU/CSU party brings together both bourgeois and traditional farmers, leaving the SPD with a smaller base in the urban working class. -The result: a set of welfare policies linked both to market performance (via indexing to past wages) and to existing class and social structures (by differentiating between working class and salaried groups, emphasizing the male breadwinner model of social provision, and mandating worker contributions) -Even under the SPD government of Brandt, major chagnes to the welfare state favored by the SPD proved difficult. Conclusions: 1. Boundaries of social citizenship vary across countries 2. The extent and nature of those boundaries appears to vary with the strength of the working class political movement 3. Thus the power-resources model has a good deal to say about how the welfare state developed in the postwar era, and why transnational variation remains today.},
	Author = {G{\"o}sta Esping-Andersen and Walter Korpi},
	Chapter = {8: Social Policy as Class Politics in Post-War Capitalism: Scandinavia, Austria, and Germany},
	Date-Added = {2008-09-14 13:24:54 -0700},
	Date-Modified = {2008-09-14 14:18:33 -0700},
	Editor = {John H. Goldthorpe},
	Keywords = {levy, origin of welfare state, power-resources models},
	Publisher = {Clarendon Press},
	Title = {Order and Conflict in Contemporary Capitalism},
	Year = {1984}}

@article{Evans:2000,
	Abstract = {Class voting is supposedly in severe decline in advanced industrial democracies. However, this conventional wisdom derives from research using prob- lematic methods and measures and an overly simple model of political change. This chapter overviews past and current comparative research into changes in and expla- nations of class-based political behavior and argues for the continued significance of class voting and, by extension, class politics in contemporary democracies. I partic- ularly emphasize the importance of using more appropriate methods and the appli- cation and testing of theories that integrate developments in this area with those in studies of voting behavior more generally. This translates into a need for the sys- tematic testing of bottom-up/top-down interactions in the relations between social structure and political preferences and the precise specification and measurement of explanatory mechanisms that can account for the association between class position and voting. },
	Annote = {Argument from method: argues that evidence presented for the decline of class salience to electoral politics may suffer from the same problems that have always plagued the measures of class voting; that the results of these studies are thus suspect; and that the jury is still out on whether claims from Inglehart (1986) on post-materialism or other causes of the decline in class salience are well-founded. Common explanations for decline: 1. Social class has lost some or all of its importance as a determinant of life chances and in consequence its role as a source of divergent political interests. This has resulted from processes such as the ``embourgeoisement'' of the working class or the ``proletarianization'' of white-collar work, and from the presence of extensive intragenerational and intergenerational social mobility (cf Abrams et al 1960, Goldthorpe et al 1968). 2. New post-industrial social cleavages are replacing class-based conflict. The significance of class position for voting behavior has decreased as ``new'' forms of social differentiation have become more important for political interests---e.g. gender, race, ethnicity, and production and consumption sectors (cf Dunleavy 1980, Saunders 1981, Huckfeldt \& Kohfeld 1989, Heath et al 1991, Saunders 1990). 3. Because of a general increase in levels of education and ``cognitive mobilization,'' identity-based responses to class-based divisions of interest are being replaced by the expression of preferences that reflect voters' increasing ability to make electoral decisions that are calculative and issue oriented rather than being driven by collective identities (cf Franklin 1985, Rose \& McAllister 1986, Dalton 1996, Heath et al 1991). 4. Values are becoming more important as a basis of party preference and are cross-cutting the impact of social class. In particular, the increasing influence of post-material values has led to a decline in the importance of the traditional, class-based, left-right political continuum for voting behavior. The new left draws its support from the middle classes, weakening the class basis of left-right divisions (cf Lipset 1981 [1960]:509--21, Inglehart \& Rabier 1986, Weakliem 1991). 5. Finally, since the manual working class has declined as a proportion of the electorate, left-wing parties have had to direct their programs toward the concerns of the growing middle classes or face continued electoral defeat. They have therefore moderated the class character of their political appeals and weakened the class distinctiveness of the political choices facing the electorate (cf Przeworski \& Sprague 1986, King \& Wickham-Jones 1990). Problems here: 1. How is class defined 2. How is political preference defined Class voting is fairly straightforward: the propensity for members of one class to consistently vote for one party in significantly higher numbers than for hte other party or parties in the system. But useful measures of class voting must therefore measure both class and party properly. Past articles, including Alford, used fairly simple measures, and treated "left" and "right" as monolithic. This may have produced spurious results, and continue to do so. Instead, he cites studies of European and American politics suggesting that class voting has a more nuanced, and recently stronger, history than earlier studies would suggest. Those who disaggregate "right" and "left", and who make more careful distinctions about "working class" (e.g., between manual labor, skilled blue-collar labor, and blue-collar management like foremen or superintendents) find less evidence for the kinds of developments specified in 1-5 above. },
	Author = {Geoffrey Evans},
	Date-Added = {2008-09-09 22:25:25 -0700},
	Date-Modified = {2008-09-09 22:39:14 -0700},
	Journal = {Annual Reviews in Political Science},
	Keywords = {class voting, parties, post-materialist},
	Local-Url = {file://localhost/Users/markhuberty/Documents/BibtexAutofilePapers/Evans/2000.pdf},
	Number = {3},
	Pages = {401--417},
	Title = {The Continued Significance of Class Voting},
	Year = {2000},
	Bdsk-File-1 = {}}

@article{Inglehart:1986,
	Annote = {Classic article on the rise of post-materialist politics and the lack of party structures that accommodate it. Inglehart and Rabier observe that surveys of younger Europeans, and cohort analysis through time, both show signs of the decline of class politics and the rise of post-materialism (less concern for economic growth, more concern for pacifism or environmental policy or similar non-economic issues). They attribute this move to a few crucial factors: the attainment of a high level of wealth; the improved education of this generation of Europeans; the growing share of non-manual jobs or service jobs in the economy; and the consolidation of major party positions at hte center, in ways that do not provide homes for this kind of politics. New Politics parties have tended to be volatile. The 1970s saw wave of economic reform parties which then faded with the economic relief of the 1980s; the 1980s have seen ecology parties on the left and far-right/nationalist parties on the right; whether these will survive or be incorporated into existing mass parties is to be determined (nb - did not happen with the Greens, which have become a smaller generalist party that, with Joschka Fischer, showed that they could adapt to positions of leadership; has not yet happened with the FN, though the impending death of its charismatic leader and the maneuvering of Sarkozy may temper its weight after the late-1990s surge). BNP has done well; the Lib Dems have not. },
