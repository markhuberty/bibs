@article{Sandholtz1989,
	Annote = {Argument: standard theories for European integration (esp functionalism) cannot explain why the SEA / move to Maastricht appeared when and how it did. To do so, you must look to the changing international political and economic environment to understand how the policy space available to Europe changed in the 1970s and early 1980s, in ways that made policy entrepreneurship possible on the part of the Commission. Other possible arguments: 1. Functionalism assumes integration has an interior logic. But that doesn't explain why integration has been so staccato. It also doesn't account for why, in areas where the EC had no competency, national solutions continued to obtain (esp. security). 2. Domestic politics doesn't explain where the policy leadership came from. It does help to explain why such leadership could be followed. What changed: 1. The U.S. declined in relative terms in the world economy and so lost some of its hegemonic control 2. Japan emerged as a serious economic challenger to the developed economies of Europe and N. America 3. Technological evolution changed the nature of transatlantic ties; technological trade had been closely tied to security - but with Japan's rise, Europe faced technological dependency outside the security framework. 4. The dollar-based system switched from one of design to one of choice, with a commensurate decline in US market power What could Europe have chosen: 1. National responses 2. A switch from close ties to the US to close ties with Japan 3. European integration as a means of changing the terms on which Europe participated in global markets. (3) was chosen. Why? 1. Economic changes. Failure of national political-economic models in the 1970s opened the possibility of seeking an extra-national solution. 2. Demise or change in Leftist politics by 1983 eliminated a major political opponent to European integration. In other words, governmental coalitions shifted away from anti-market forces to pro-market forces (sometimes unexpectedly) who saw the common market as a way of achieving national prerogatives. 3. Demise in corporatism across Europe meant extra-national bargains became more possible. 4. Deregulation - whether under Thatcher or Mitterand - allowed previously incompatable coalitions to emerge. Amidst 1-4, the Commission under Delors was able to be a policy entrepreneur via very savvy moves to time and date the march towards a common market in 1992. In this context, it found support from powerful European MNCs. Thus you had convergence across three powerful policy actors: national governments, the Commission, and the Corporation. Question: will the logic of 1992 continue to drive a set of bargains that will be important to the functioning of the market but were not in Delors' ``300 points?'' 1. Emergence of a common monetary policy and integrated capital markets 2. A method of dealing with welfare state policy that prevents national competiton on the basis of the institutional structure of the welfare state, but does not require harmonization of standards. 3. Defense. 4. Community vs. National decision-making },
	Author = {Wayne Sandholtz and John Zysman},
	Date-Added = {2007-10-30 13:47:39 -0700},
	Date-Modified = {2007-10-30 13:48:56 -0700},
	Journal = {World Politics},
	Keywords = {zysman, ps201w10},
	Month = {October},
	Number = {1},
	Pages = {95--128},
	Title = {1992: Recasting the European Bargain},
	Volume = {42},
	Year = {1989}}

@inbook{Garrett1993,
	Address = {Ithaca and London},
	Annote = {Question: why did the EU coalesce around a model of mutual recognition for the internal market, given that there were several other possible and efficient solutions available? Cannot be explained by functionalist ideas because of the possibility of multiple equilibria; cannot be explained by standard models of cooperation for mutual gain, since it's not clear why all actors would converge on the same option for cooperation. Argument: in an environment where cooperation would produce gains but information asymmetry between actors is high, ideas can play a critical role in constructing ``focal points'' around which different actors can all converge. In the case of the Common Market, the idea was the decision endorsing ``Mutual Recognition'' in the EU, in the Cassis de Dijon ruling of ECJ. By finding mutual recognition to be required by the Treaty of Rome, the ECJ both (1) embedded the idea in the larger European project, thereby making exit harder, and (2) injected an idea for how to run the common market in an environment where there were already interests supported the common market in principle. Thus Cassis de Dijon gave impetus to a political settlement that hte actors might not have been able to reach on their own, given the lack of complete transparency between them. Note however: there are limits to the range of ideas that work in this context. The ECJ has limited enforcement mechanisms and therefore must be careful to ground its rulings in prevailing political winds. If it pushes too hard on sensitive issues, the member state governments can push back, up to and including revising the Treaty of Rome. Thus the ECJ works as a source of idea ``focal points'' only when it operates within the prevailing political winds.},
	Author = {Geoffrey Garrett and Barry R. Weingast},
	Chapter = {7: Ideas, Interests, and Institutions: Constructing the European Community's Internal Market},
	Date-Added = {2007-10-30 13:31:52 -0700},
	Date-Modified = {2007-10-30 13:42:21 -0700},
	Editor = {Judith Goldstein and Robert Keohane},
	Keywords = {zysman, ps201w10},
	Publisher = {Cornell University Press},
	Title = {Ideas and Foreign Policy},
	Year = {1993}}

@inbook{Pempel1997,
	Address = {Ithaca and London},
	Annote = {Argument: that Asia is converging on a regionalist economy, but one that is (1) not constituted as a more peaceful version of the Japan-centric ``Greater East Asian Co-Prosperity Sphere'' and (2) not a closed model. Brief historical sketch: For both economic and political reasons, Japan quickly became the largest foreign aid and FDI source for Asia after WWII. This was both because of the need to pay reparations, and as a way of building market access. Early on, this model of relations with its neighbors developed into a ``flying goose'' model where the less-developed neighbors were expected to adopt mfg of products after Japan had moved up the value chain. Thus Japan would be the leader, SE Asia the followers. Where this would go depended on internal Japanese politics. Right-wing nationalism saw paternal relations with Asia as a way of reasserting Japan's honor and prestige after WWII; left-wing socialism preferred neutrality and a severing of ties with the international capitalist economy. What won out was a more centrist idea of Japanese politics. In turn, this meant close ties with the U.S., and peripheral ties with Asia. Economic relations were structured to import raw materials from Asia and export finished goods to the U.S. Moves away from this model came with the successful reindustrialization of Japan and the end of Bretton Woods. Japanese firms with capital to spare turned to FDI in Asia as a way of establishing lower-cost supply chains. FDI also helped get around problems of currency appreciation, by producing directly for markets. In time, Japanese FDI became the major source of foreign investment, ahead even of the U.S. As such, the second period of postwar history sees Japan becoming ``more Asian.'' Nevertheless, it remained closely linked - in security, economics, and politics - to the U.S. But this greater ``Asianism'' within Japan was not comprehensive. What happened in economics did not happen in culture - and so Japan's influence on Asia compared with the U.S. remained checked, because of U.S. cultural dominance. Thus by the end of the 20th century, Asia has economic regionalism embedded in economic transnationalism. Japan sits atop a structure of production where raw materials and intermediates are fed to it from Asia, and manufactures go to the developed world. This portends a future of ``internationalist'' regionalism. (NOTE - why? Because Japan lacked the ability to pull off the cultural / political reconciliation that made the EU possible...)},
	Author = {T.J. Pempel},
	Chapter = {1: Transpacific Torii: Japan and the Emerging Asian Regionalism},
	Date-Added = {2007-10-30 13:18:55 -0700},
	Date-Modified = {2007-10-30 13:20:34 -0700},
	Editor = {Peter J. Katzenstein and Takashi Shiraishi},
	Keywords = {zysman, ps201w10},
	Publisher = {Cornell University Press},
	Title = {Network Power: Japan and Asia},
	Year = {1997}}

