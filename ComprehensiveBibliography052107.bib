@article{Olson1993,
	Annote = {Argument: the conditions necessary for lasting democracy are the same as those for the security of property and contract rights at the heart of economic growth. Note that autocrats and their subjects can be construed as rational actors. It is rational, if you live in a time of great uncertainty, to tolerate the autocrat's demands for taxation in exchange for physical security. Likewise, if you are an autocrat, it is rational to limit your demands for taxation to some optimum but not confiscatory level, lest your subjects grow weary and find another autocrat. However, this optimum is probably above the level at which sustained economic growth becomes possible. This is because the autocrat, knowing the violence of the transition of power, has good reason to amass wealth for a day when he must flee, or to spend it now since he'll be killed eventually. He thus has an incentive to provide public goods (security) and to extract the maximum amount for himself (rent-seeking). In democracies, the level of tolerance is lower. Why? Because property rights give the ultimate rulers an interest in their private incomes. Since taxation, taken to the level of monopoly rent, reduces the private income, the majority is less likely to tax itself at that level. (NOTE: Olson seems a bit circular. You need private property to protect democracy; but private property \&c is politically grounded. So you need democracy to protect private property. This is an infinite regression). He says that the establishment of reliable property rights lengthens time horizons of political leaders and informs against monopoly rent-seeking, and therefore provides the grounds for economic growth. But likewise, those rights must come from somewhere - and he says that somewhere is politics! So what comes first? },
	Author = {Mancur Olson},
	Date-Added = {2007-10-24 09:53:05 -0700},
	Date-Modified = {2007-10-24 09:53:54 -0700},
	Journal = {American Political Science Review},
	Keywords = {fish, ps200w9},
	Month = {September},
	Number = {3},
	Title = {Dictatorship, Democracy, and Development},
	Volume = {87},
	Year = {1993}}

@inbook{Popkin1988,
	Address = {Berkeley},
	Annote = {Problem: lots of things useful for peasants require collective action: crop insurance, social protection, protection vs. landlords, etc. But collective action is difficult in a rational choice paradigm, and faces increasing problems as the size of the peasant group goes up. First, there are several reasons for why collective goods might exist despite rat choice assumptions: 1. Ethics / conscience / altruism 2. Cost/ben of contribution is positive 3. Selective incentives overcome collective action costs 4. Contributions of others are contingent on one's own contribution. Participation will also depend on the individual's circumstance. If there is risk to collective action, individuals with surplusses are more likely to participate than those operating at subsistence. Role of leadership: leaders can solve the collective action problem through credible promises to deal with its negative results (e.g., freeriding, enforcement, excludability). But they too face rational action constraints: 1. Masses must believe leaders are not acting in pure self-interest 2. Leaders must convince people of their ability to lead (and thus to deal with collective action negatives) 3. Leaders must be able to articulate benefits of collective action such that they are apparent to those who otherwise would not recognize them. },
	Author = {Samuel L. Popkin},
	Chapter = {7: Public Choice and Peasant Organization},
	Date-Added = {2007-10-24 09:45:08 -0700},
	Date-Modified = {2007-10-24 09:46:26 -0700},
	Editor = {Robert H. Bates},
	Keywords = {fish, ps200w9},
	Publisher = {University of California Press},
	Title = {Toward a Political Economy of Development: A Rational Choice Perspective},
	Year = {1988}}

@book{Levi1997,
	Address = {New York},
	Annote = {Contributions of rat choice: 1. Realization that individual and collective best interest can be incongruous 2. Enabling of theory testing 3. Move to causal inference instead of mere correlation Origins: Neoclassical economics' assumptions about utility-maximizing individuals. Difference - that unlike in markets, in politics you don't necessarily get Pareto-optimal outcomes from each individual acting in their own self-interest (collective action problems). Furthermore, the presence of institutions of varying kinds alters the individual's perception of their self-interest. The Model: 1. Links microprocesses to macroprocesses and events 2. Assumptions defined ex ante (reduces explanatory power but allows testability) 3. Relies on constraints to create incentives and disincentives 4. Assumes strategic action: that is, an individual's decision is based on optimization within a landscape of other individuals' decisions 5. The system tends towards equilibrium. Hence changes to one part of the system will set off processes to reorder the equilibrium state. The modes of analysis 1. Path Dependency: Given steps down a path, reversal costs are high. Why? Because individual expectations are shaped by those initial steps, which means their rational decision-making is shaped by those steps. Moving off the path requires stepping back from this decision-making context and finding another context in which to see a given set of problems. This is very hard, and potentially very costly. 2. Analytic narratives that assume that outcomes are derived from aggregated individual choices. Problems: 1. Reliance on stylized facts, inherited from econ 2. Rat choice is not implicitly valid, even when its predictions fit the facts 3. Hard to account for large-scale mass action in teh face of the collective action problem 4. Not clear how immediately policy-relevant its conclusions are (3) is most difficult. We know that mass action occurs, even though collective action suggests it shouldn't. Suggests that we don't actually understand things like altruism / ethics / social concern well enough to know how these factors counteract selfishness. },
	Author = {Margaret Levi},
	Date-Added = {2007-10-24 09:30:27 -0700},
	Date-Modified = {2007-10-24 09:31:41 -0700},
	Editor = {Mark Irving Lichbach and Alan L. Zuckerman},
	Keywords = {fish, ps200w9},
	Publisher = {Cambridge University Press},
	Title = {Comparative Politics},
	Year = {1997}}

@book{Tsebelis1990,
	Address = {Berkeley},
	Annote = {Rationality: optimal correspondence between ends, means. Thus, an actor should reasonably suppose that there are solid links between my a priori judgement of the situation and outcomes from applying that judgement. What Rat Choice is not: 1. Actorless (as in structuralism or institutionalism) 2. Irrational, in the sense of denying links between means and ends. What Rat Choice is: 1. Weak Requirements: a. No contradictory preferences b. No intransitivity (e.g. if a>b and b>c, a>c) c. Probability-based calculus 2. Strong requirements: Strategic interaction and equilibrium a. Strategies of interacting players are mutually optimal when in equilibrium b. Probabilities approximate reality in equilibrium (e.g., there is convergence in reality and the perception of reality) c. Beliefs approximate reality in equilibrium Argument that Rat Choice should be taken as correct not simply because it makes accurate predictions (which could leave open the question of whether people really are rational) but because its assumptions about behavior are correct. Why should we believe this: 1. People learn. When assumptions don't match reality, they tend to readjust their assumptions. Over time, this implies convergence. 2. Not everyone has to be rational in the beginning to achieve ostensibly rational outcomes (e.g., if even a subset are rational, they will choose optimizing conditions in such a way that will lead to equilibrium that is optimizing for all. Labor-markets as an example.) 3. Natural selection should reinforce rational behavior 4. Rat Choice is only concerned about rationality of hte population - not of every individual. What matters is the mean level of rationality, not the components of that mean. Why should we deal with the continual re-sorting of assumptions and ex ante preferences required by rat choice? 1. Allows us to understand how features of society - institutions - shape individual behavior through constraints. 2. Four major advantages: parsimony, equilibrium analysis, deductive reasoning, and generalization. Parsimiony: outcomes explained as deliberate choices, with testable propositions about why those choices were made Equilibrium: allows understanding of change; if we assume things tend towards equilibrium, then by starting at a equil. and changing features of the system, we assume that things will change to some future equil. Deduction: Allows us to reason from a set of assumptions; mismatches between deduction and reality provide inputs on how to modify those assumptions. This is a process of knowledge accumulation. Generalization: moves us beyond discussion of immutable differences like culture. },
	Author = {George Tsebelis},
	Date-Added = {2007-10-24 09:11:00 -0700},
	Date-Modified = {2007-10-24 09:13:25 -0700},
	Keywords = {fish, ps200w9},
	Publisher = {University of California Press},
	Title = {Nested Games: Rational Choice in Comparative Politics},
	Year = {1990}}

@book{Riker1990,
	Address = {Cambridge},
	Annote = {Argument: political science has developed slowly compared with the natural sciences because it faces more difficulty in classifying its objects of study and making clear statements about how those objects behave at a micro level. Rat Choice, by offering a microfoundational explanation for these objects, has assisted the rapid development of political science. Problem is ambiguity: social science is bad at being unambiguous in its definitions. Where this is least problematic is economics - because it can study prices and quantities, which are small, highly defined units. From there, generalization is easier. Lacking these features, generalization is only possible if you can assume common behaviors across your objects of study. In this case, rational choice, by assuming rationality across its subjects, allows for generalization. Defining Features of Rat Choice: 1. Individuals see the world as a set of ordered preferences 2. Choice from among these preferences is made to so to maximize utility Note what this doesn't require: that the preferences be complete; that the actor's information about the preferences be complete; or that the preferences correspond to the ultimate goals of the individual. These two features give rat choice internal coherence. Since common behaviors are assumed for all actors, we can assume that transplanting people into given sets of circumstances will result in similar reactions. },
	Author = {William Riker},
	Date-Added = {2007-10-24 09:01:24 -0700},
	Date-Modified = {2007-10-24 09:02:40 -0700},
	Editor = {James Alt and Kenneth Shepsie},
	Keywords = {fish, ps200w9},
	Publisher = {Cambridge University Press},
	Title = {Perspectives on Positive Political Economy},
	Year = {1990}}

@techreport{Schulze-Cleven2007,
	Address = {Berkeley},
	Annote = {QUESTION: If Fordism required Keynesianism, how to explain 1900-1950? Real keynesian politics didn't arrive in the US in non-crisis form until the postwar era. Prewar / pre-1929 politics were classical monetarist, as in Hoover. So clearly something else must have been present during this period if the market was as rigid as they claim. Argument: the changing international logic of production means that social protection systems, properly understood, have a critical role to play in reorganizing economic structures in a fluid global economy. Cf. Denmark (flexicurity) vs. Germany (rigidity). Old system: Keynesian Fordism. Fordist manufacturing generated large economies of scale but economic rigidities. Keynesian demand management and unemployment protection mitigated the effects of the rigidities (in particular the recessions and layoffs that ran through the business cycle). The advent of lean production (Japan) and flexible specialization / diversified quality production (Europe) threatened the US Fordist system in the 1980s. Both had different roles for the state than the Keynesian Fordist Model. Wintelism and CPNs imply a new set of changes for the role of the state in policy. The state encouraged it by deregulating the IT and telecom markets (IBM and AT\&T). The result was a disaggregated economy. Once that happened, it became much easier to move functions internationally, setting the stage for a new set of challenges to the state and the labor market. How do states compete when everything becomes mobile? (Thus - modularization of production / corporate activity + algorithmic revolution allowing increasing unbundling drives changing landscape for national economic advantage.) Firm-level response: Shift from services that are valuable because they link to manufactures to manufactures that are valuable because they have links to services (e.g. the IBM 360 vs. the iPod). How do states respond to the unbundling / digital change element of the economy? -NO apparent link between equity and efficiency, as suggested by the nordic economies. -Social protection, if it buffers against mass economic dislocation, can make competition easier. E.g., the Danish system both cushions against dislocation and provides retraining. Where social systems are a problem is when they ``protect against change'' rather than ``protect for change.'' Because they tend to concentrate individuals in sectors of an economy, they delay change rather than promote adaptation. Hiring / firing rules are one example. },
	Author = {Tobias Schulze-Cleven and Bartholemew C. Watson and John Zysman},
	Date-Added = {2007-10-23 09:52:48 -0700},
	Date-Modified = {2008-05-14 15:13:52 -0700},
	Institution = {Berkeley Roundtable on the International Economy},
	Keywords = {zysman, ps201w9},
	Local-Url = {file://localhost/Users/markhuberty/Documents/BibtexAutofilePapers/Schulze-Cleven/2007.pdf},
	Month = {May},
	Number = {177},
	Title = {Innovation and Adaptability in a Digital Era: How Wealthy Nations Stay Wealthy},
	Type = {BRIE Working Paper},
	Year = {2007},
	Bdsk-File-1 = {}}

@techreport{Borrus1997,
	Address = {Berkeley},
	Annote = {Two developments shape significant changes in the global economy: 1. Wintelism: the changing logic of market control, shifting control away from vertically integrated production networks and to control over market-set standards for components. The result changes where value is created in the market, how products are marketed, and how firms compete. 2. Cross-national production networks: the ability to separate out stages of production allows countries to specialize in these stages, and for firms to build international production networks on the basis of national specialization. This reduces the demand for vertically integrated production. It also means that firms become much more flexible, but also much less tied into their home economies. Manufacturing becomes a commodity service, not a competitive advantage. Advantage derives from the ability to organize and control production chains, and to embed value in commodified products. The result: national comparative advantage still matters in an age of globalized production, but national specialization puts new demands on politics. Pre-Wintelism: vertically integrated production allowed capture of markets via 2 mechanisms: proprietary standards and rent capture (e.g., IBM 360 mainframe technology and all the stuff that went with it); or control over the entire production process to allow price-quality - based competition. Source of change: disaggregation of IBM and AT\&T via antitrust law pushed technology out into the market and forced these firms to rely on production networks. Wide range of firms resulted, competing in all aspects of electronics and software production. In turn, these firms could recombine in various ways to produce new products using the same sets of technology. Network-driven manufacturing meant standards setting was thrown to the market. Market dominance now came not through vertical control of propietary standards, but through control of the IP and licensing of standards. A new mode of competition emerged. What now mattered was compliance with this broader network of products. The final ``box'' mattered only insofar as it plugged into this set of standards. Hence the dominance of the Wintel paradigm and its importance to marketing. The specific features of American antitrust policy meant that US firms were uniquely positioned to accomplish this change in production logic, in the same way that Japanese firms were uniquely positioned to create lean production. CPNs Wintelism = shift of prodution to rapid evolution on the basis of standardized components that evolve rapidly in accordance with market-driven standards. CPNs are an extension of that logic, in which the different demands of various component manufacture create opportunities to place entire nations at specific points in the production chain (NOTE: significant that these are small nations. It's one thing for Taiwan to become the world DRAM leader; this is not an option for the US) Thus mfg becomes a service. Value lies in design; mfg is contracted out. Control over mfg still important, but in the sense of control over the production chain, not control over the actual means of production. Likewise, control of standards still important, but in the sense of organizing the coalitions necessary to protect a given market-set standard, not in controlling a proprietary standard. Finally, seem to be little reason to believe that Wintel forms of innovation and production won't diffuse transnationally and beyond the computer / IT world. NOTE: OK so far as this goes, but it depends on a few things: 1. Free trade 2. Cheap fuel 3. Relative peace 4. Means of contract enforcement to control CPNs Not at all clear that these things are changed by the rise of different means of production. And the forces unleashed by wintelism may make it more difficult to guarantee some of these; (1) in particular.},
	Author = {Michael Borrus and John Zysman},
	Date-Added = {2007-10-23 09:16:12 -0700},
	Date-Modified = {2007-10-23 09:40:36 -0700},
	Institution = {Berkeley Roundtable on the International Economy},
	Keywords = {zysman, ps201w9},
	Month = {February},
	Number = {96B},
	Title = {Wintelism and the Changing Terms of Global Competition: Prototype of the Future},
	Year = {1997}}

@techreport{Zysman2006,
	Abstract = {Argument that the ability to encode business logic in software enables the delivery of services either without human intervention or with human intervention from afar, thus making services tradable and therefore changing the logic of market competition in the services sector. This will make it harder for the advanced economies to develop durable national modes of economic competition, since increasing ID capabilities will expose an increasing number of sectors and skills to global competition. It will also make social policy harder, since the cleavages between winners and losers from globalization will become harder to predict and insure against. 4 transformations: 1. Firm-level outsourcing of services: moving employees from in-house to contractor services 2. Changes in consumer purchasing to increase the share of services 3. Conversion of unpaid domestic work to commercial services 4. The digitial transformation},
